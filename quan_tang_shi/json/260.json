[
    {
        "title": "晚秋拾遺朱放訪山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "不逐時人後，終年獨閉關。家中貧自樂，石上臥常閑。",
            "墜栗添新味，寒花帶老顏。侍臣當獻納，那得到空山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 1
    },
    {
        "title": "題女道士居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "不餌住雲溪，休丹罷藥畦。杏花虛結子，石髓任成泥。",
            "掃地青牛臥，栽松白鶴棲。共知仙女麗，莫是阮郎妻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 2
    },
    {
        "title": "山中枉張宙員外書期訪衡門",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "常恨相知晚，朝來枉數行。臥雲驚聖代，拂石候仙郎。",
            "時果連枝熟，春醪滿甕香。貧家仍有趣，山色滿湖光。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 3
    },
    {
        "title": "山中贈張正則評事",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "終年常避喧，師事五千言。流水閑過院，春風與閉門。",
            "山茶邀上客，桂實落前軒。莫強教餘起，微官不足論。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 4
    },
    {
        "title": "題鏡湖野老所居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "湖裏尋君去，樵風往返吹。樹喧巢鳥出，路細葑田移。",
            "漚苧成魚網，枯根是酒卮。老年唯自適，生事任群兒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 5
    },
    {
        "title": "早秋宿崔業居處",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "從來席不暖，為爾便淹留。雞黍今相會，雲山昔共遊。",
            "上簾宜晚景，臥簟覺新秋。身事何須問，餘心正四愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 6
    },
    {
        "title": "贈烏程楊蘋明府",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "策杖政成時，清溪弄釣絲。當年潘子貌，避病沈侯詩。",
            "漉酒迎賓急，看花署字遲。楊梅今熟未，與我兩三枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 7
    },
    {
        "title": "徐侍郎素未相識時攜酒命饌兼命諸詩客同訪會稽山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "忽道仙翁至，幽人學拜迎。華簪窺甕牖，珍味代藜羹。",
            "洗硯魚仍戲，移樽鳥不驚。蘭亭攀敘卻，會此越中營。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 8
    },
    {
        "title": "春日閒居三首",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "一似桃源隱，將令過客迷。礙冠門柳長，驚夢院鶯啼。",
            "澆藥泉流細，圍棋日影低。舉家無外事，共愛草萋萋。",
            "長謠朝複暝，幽獨幾人知。老鶴兼雛弄，叢篁帶筍移。",
            "白雲將袖拂，青鏡出簷窺。邀取漁家叟，花間把酒卮。",
            "寂寂池亭裏，軒窗間綠苔。遊魚牽荇沒，戲鳥踏花摧。",
            "小徑僧尋去，高峰鹿下來。中年曾屢辟，多病複遲回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 9
    },
    {
        "title": "題石室山王甯所居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "白雲知所好，柏葉幸加餐。石鏡妻將照，仙書我借看。",
            "鳥來翻藥碗，猿飲怕魚竿。借問簷前樹，何枝曾掛冠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 10
    },
    {
        "title": "送王道士",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "真人俄整舄，雙鶴屢飛翔。恐入壺中住，須傳肘後方。",
            "霓裳雲氣潤，石徑術苗香。一去何時見，仙家日月長。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 11
    },
    {
        "title": "將移耶溪舊居留贈嚴維秘書",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "雞犬漁舟裏，長謠任興行。那邀落日醉，已被遠山迎。",
            "書篋將非重，荷衣著甚輕。謝安無個事，忽起為蒼生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 12
    },
    {
        "title": "秋日過僧惟則故院",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "衰草經行處，微燈舊道場。門人失譚柄，野鳥上禪床。",
            "科鬥書空古，栴檀缽自香。今朝數行淚，卻灑約公房。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 13
    },
    {
        "title": "山中奉寄錢起員外兼簡苗發員外",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "空山歲計是胡麻，窮海無梁泛一槎。稚子唯能覓梨栗，",
            "借問省中何水部，今人幾個屬詩家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 14
    },
    {
        "title": "獻薛僕射",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "由來那敢議輕肥，散發行歌自采薇。逋客未能忘野興，",
            "更乞大賢容小隱，益看愚谷有光輝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 15
    },
    {
        "title": "鮑防員外見尋因書情呈贈",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "少小為儒不自強，如今懶複見侯王。覽鏡已知身漸老，",
            "猶有郎官來問疾，時人莫道我佯狂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 16
    },
    {
        "title": "寄浙東皇甫中丞",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "閑閑麋鹿或相隨，一兩年來鬢欲衰。琴硯共依春酒甕，",
            "久帶紗巾仍藉草，山中那得見朝儀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 17
    },
    {
        "title": "題章野人山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "帶郭茅亭詩興饒，回看一曲倚危橋。門前山色能深淺，",
            "向此隱來經幾載，如今已是漢家朝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 18
    },
    {
        "title": "山中枉皇甫溫大夫見招書",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "十年木屐步苔痕，石上松間水自喧。三辟草堂仍被褐，",
            "亞相已能憐潦倒，山花笑處莫啼猿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 19
    },
    {
        "title": "題茅山李尊師山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "天師百歲少如童，不到山中竟不逢。洗藥每臨新瀑水，",
            "此去人寰今遠近，回看去壑一重重。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 20
    },
    {
        "title": "耶溪書懷寄劉長卿員外",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "時人多笑樂幽棲，晚起閑行獨杖藜。雲色卷舒前後嶺，",
            "擬共釣竿長往復，嚴陵灘上勝耶溪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 21
    },
    {
        "title": "山中崔大夫有書相問",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "客在煙霞裏，閑閑逐狎鷗。終年常裸足，連日半蓬頭。",
            "帶月乘漁艇，迎寒綻鹿裘。已於人事少，多被掛冠留。",
            "素業堆千卷，清風至一丘。蒼黃倒藜杖，傴僂睹銀鉤。",
            "跡愧巢由隱，才非管樂儔。從來自多病，不是傲王侯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 22
    },
    {
        "title": "張建封大夫奏系為校書郎因寄此作",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "久是煙霞客，潭深釣得魚。不知芸閣上，遺校幾多書。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 23
    },
    {
        "title": "會稽山居寄薛播侍郎袁高給事高參舍人",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "稷契今為相，明君複為堯。寧知買臣困，猶負會稽樵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 24
    },
    {
        "title": "閒居覽史",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "長策胸中不復論，荷衣藍縷閉柴門。",
            "當時漢祖無三傑，爭得咸陽與子孫。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 25
    },
    {
        "title": "山中贈耿拾遺湋兼兩省故人",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "數片荷衣不蔽身，青山白鳥豈知貧。",
            "如今非是秦時世，更隱桃花亦笑人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 26
    },
    {
        "title": "秋日送僧志幽歸山寺",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "禪室繩床在翠微，松間荷笠一僧歸。",
            "磬聲寂曆宜秋夜，手冷燈前自衲衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 27
    },
    {
        "title": "題僧明惠房",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "簷前朝暮雨添花，八十真僧飯一麻。",
            "入定幾時將出定，不知巢燕汙袈裟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 28
    },
    {
        "title": "答泉州薛播使君重陽日贈酒",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "欲強登高無力也，籬邊黃菊為誰開。",
            "共知不是潯陽郡，那得王弘送酒來。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 29
    },
    {
        "title": "題洪道士山院",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "霞外主人門不扃，數株桃樹藥囊青。",
            "閑行池畔隨孤鶴，若問多應道姓丁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 30
    },
    {
        "title": "期王煉師不至",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "黃精蒸罷洗瓊杯，林下從留石上苔。",
            "昨日圍棋未終局，多乘白鶴下山來。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 31
    },
    {
        "title": "題贈張道士山居",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "磐石垂蘿即是家，回頭猶看五枝花。",
            "松間寂寂無煙火，應服朝來一片霞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 32
    },
    {
        "title": "山中書懷寄張建封大夫",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "昨日年催白髮新，身如麋鹿不知貧。",
            "時時亦被群兒笑，賴有南山四老人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 33
    },
    {
        "title": "山中贈諸暨丹丘明府",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "荷衣半破帶莓苔，笑向陶潛酒甕開。",
            "縱醉還須上山去，白雲那肯下山來。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 34
    },
    {
        "title": "奉寄晝公",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "蓑笠雙童傍酒船，湖山相引到房前。",
            "團蕉何事教人見，暫借空床守坐禪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 35
    },
    {
        "title": "宿雲門上方",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "禪室遙看峰頂頭，白雲東去水長流。",
            "松間倘許幽人住，不更將錢買沃州。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 36
    },
    {
        "title": "即事奉呈郎中韋使君",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "久臥雲間已息機，青袍忽著狎鷗飛。",
            "詩興到來無一事，郡中今有謝玄暉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 37
    },
    {
        "title": "曉雞",
        "author": "秦系",
        "biography": "",
        "paragraphs": [
            "黯黯嚴城罷鼓鼙，數聲相續出寒棲。",
            "不嫌驚破紗窗夢，卻恐為妖半夜啼。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷二百六十",
        "no#": 38
    }
]