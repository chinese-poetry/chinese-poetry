[
    {
        "title": "送李億東歸",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "黃山遠隔秦樹，紫禁斜通渭城。別路青青柳弱，",
            "灞上金樽未飲，宴歌已有餘聲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 1
    },
    {
        "title": "開聖寺",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "路分溪石夾煙叢，十裏蕭蕭古樹風。出寺馬嘶秋色裏，",
            "猶有南朝舊碑在，恥將興廢問休公。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 2
    },
    {
        "title": "贈蜀府將",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "十年分散劍關秋，萬事皆隨錦水流。志氣已曾明漢節，",
            "今日逢君倍惆悵，灌嬰韓信盡封侯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 3
    },
    {
        "title": "西江貽釣叟騫生",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "晴江如鏡月如鉤，泛灩蒼茫送客愁。衣淚潛生竹枝曲，",
            "昨日歡娛竟何在，一枝梅謝楚江頭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 4
    },
    {
        "title": "寄清源寺僧",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "石路無塵竹徑開，昔年曾伴戴顒來。窗間半偈聞鐘後，",
            "白蓮社裏如相問，為說遊人是姓雷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 5
    },
    {
        "title": "重游圭峰宗密禪師精廬",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "百尺青崖三尺墳，微言已絕杳難聞。戴顒今日稱居士，",
            "故山弟子空回首，蔥嶺唯應見宋雲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 6
    },
    {
        "title": "題李處士幽居",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "水玉簪頭白角巾，瑤琴寂曆拂輕塵。濃陰似帳紅薇晚，",
            "南山自是忘年友，谷口徒稱鄭子真。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 7
    },
    {
        "title": "利州南渡",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "澹然空水對斜暉，曲島蒼茫接翠微，波上馬嘶看棹去，",
            "誰解乘舟尋範蠡，五湖煙水獨忘機。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 8
    },
    {
        "title": "贈李將軍",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "誰言荀羨愛功勳，年少登壇眾所聞。曾以能書稱內史，",
            "不學龍驤畫山水，醉鄉無跡似閑雲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 9
    },
    {
        "title": "寒食日作",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "紅深綠暗徑相交，抱暖含芳披紫袍。彩索平時牆婉娩，",
            "自有玉樓春意在，不能騎馬度煙郊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 10
    },
    {
        "title": "李羽處士寄新醞走筆戲酬",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "高談有伴還成藪，沉醉無期即是鄉。已恨流鶯欺謝客，",
            "所恨玳筵紅燭夜，草玄寥落近回塘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 11
    },
    {
        "title": "郊居秋日有懷一二知己",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "稻田鳧雁滿晴沙，釣渚歸來一徑斜。門帶果林招邑吏，",
            "自笑謾懷經濟策，不將心事許煙霞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 12
    },
    {
        "title": "題友人池亭",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "月榭風亭繞曲池，粉垣回互瓦參差。侵簾片白搖翻影，",
            "山翁醉後如相憶，羽扇清樽我自知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 13
    },
    {
        "title": "南湖",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "湖上微風入檻涼，翻翻菱荇滿回塘。野船著岸偎春草，",
            "飄然篷艇東歸客，盡日相看憶楚鄉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 14
    },
    {
        "title": "贈袁司錄",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "一朝辭滿有心期，花發楊園雪壓枝。劉尹故人諳往事，",
            "記得襄陽耆舊語，不堪風景峴山碑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 15
    },
    {
        "title": "題西明寺僧院",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "曾識匡山遠法師，低松片石對前墀。為尋名畫來過院，",
            "自知終有張華識，不向滄洲理釣絲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 16
    },
    {
        "title": "偶題",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "微風和暖日鮮明，草色迷人向渭城。吳客捲簾閑不語，",
            "自恨青樓無近信，不將心事許卿卿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 17
    },
    {
        "title": "寄湘陰閻少府乞釣輪子",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "釣輪形與月輪同，獨繭和煙影似空。若向三湘逢雁信，",
            "終日垂鉤還有意，尺書多在錦鱗中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 18
    },
    {
        "title": "哭王元裕",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "聞說蕭郎逐逝川，伯牙因此絕清弦。柳邊猶憶青驄影，",
            "他時若到相尋處，碧樹紅樓自宛然。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 19
    },
    {
        "title": "法雲寺雙檜",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "晉朝名輩此離群，想對濃陰去住分。題處尚尋王內史，",
            "一下南台到人世，曉泉清籟更難聞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 20
    },
    {
        "title": "送陳嘏之侯官兼簡李常侍",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "縱得步兵無綠蟻，不緣句漏有丹砂。殷勤為報同袍友，",
            "梅仙自是青雲客，莫羨相如卻到家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 21
    },
    {
        "title": "春日野行",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "雨漲西塘金堤斜，碧草芊芊晴吐芽。野岸明媚山芍藥，",
            "何事輕橈句溪客，綠萍方好不歸家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 22
    },
    {
        "title": "溪上行",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "綠塘漾漾煙濛濛，張翰此來情不窮。雪羽褵褷立倒影，",
            "心羨夕陽波上客，片時歸夢釣船中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 23
    },
    {
        "title": "投翰林蕭舍人",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "人間鵷鷺杳難從，獨恨金扉直九重。萬象晚歸仁壽鏡，",
            "每過朱門愛庭樹，一枝何日許相容。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 24
    },
    {
        "title": "春日偶作",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "西園一曲豔陽歌，擾擾車塵負薜蘿。自欲放懷猶未得，",
            "釣渚別來應更好，春風還為起微波。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 25
    },
    {
        "title": "春暮宴罷寄宋壽先輩",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "斜掩朱門花外鐘，曉鶯時節好相逢。窗間桃蕊宿妝在，",
            "誰憐芳草生三徑，參佐橋西陸士龍。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 26
    },
    {
        "title": "馬嵬驛",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "穆滿曾為物外游，六龍經此暫淹留。返魂無驗青煙滅，",
            "甘泉不復重相見，誰道文成是故侯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 27
    },
    {
        "title": "和道溪君別業",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "積潤初銷碧草新，鳳陽晴日帶雕輪。風飄弱柳平橋晚，",
            "花房透露紅珠落，蛺蝶雙飛護粉塵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 28
    },
    {
        "title": "奉天西佛寺",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "憶昔狂童犯順年，玉虯閒暇出甘泉。宗臣欲舞千鈞劍，",
            "至今南頓諸耆舊，猶指榛蕪作弄田。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 29
    },
    {
        "title": "題望苑驛",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "弱柳千條杏一枝，半含春雨半垂絲。景陽寒井人難到，",
            "分明十二樓前月，不向西陵照盛姬。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 30
    },
    {
        "title": "寄分司元庶子兼呈元處士",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "閉門高臥莫長嗟，水木凝暉屬謝家。緱嶺參差殘曉雪，",
            "月榭知君還悵望，碧霄煙闊雁行斜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 31
    },
    {
        "title": "題柳",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "楊柳千條拂面絲，綠煙金穗不勝吹。香隨靜婉歌塵起，",
            "千門九陌花如雪，飛過宮牆兩自知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 32
    },
    {
        "title": "和友人悼亡",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "玉貌潘郎淚滿衣，畫羅輕鬢雨霏微。紅蘭委露愁難盡，",
            "春來多少傷心事，碧草侵階粉蝶飛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 33
    },
    {
        "title": "李羽處士故里",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "柳不成絲草帶煙，海槎東去鶴歸天。愁腸斷處春何限，",
            "終知此恨銷難盡，辜負南華第一篇。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 34
    },
    {
        "title": "卻經商山寄昔同行人",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "曾道逍遙第一篇，爾來無處不恬然。便同南郭能忘象，"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 35
    },
    {
        "title": "池塘七夕",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "月出西南露氣秋，綺羅河漢在斜溝。楊家繡作鴛鴦幔，",
            "萬家砧杵三篙水，一夕橫塘似舊遊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 36
    },
    {
        "title": "偶游",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "曲巷斜臨一水間，小門終日不開關。紅珠鬥帳櫻桃熟，",
            "與君便是鴛鴦侶，休向人間覓往還。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 37
    },
    {
        "title": "寄河南杜少尹",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "十載歸來鬢未凋，玳簪珠履見常僚。豈關名利分榮路，",
            "夕陽亭畔山如畫，應念田歌正寂寥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 38
    },
    {
        "title": "贈知音",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "翠羽花冠碧樹雞，未明先向短牆啼。窗間謝女青蛾斂，",
            "景陽宮裏鐘初動，不語垂鞭上柳堤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 39
    },
    {
        "title": "過陳琳墓",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "曾於青史見遺文，今日飄蓬過古墳。詞客有靈應識我，",
            "莫怪臨風倍惆悵，欲將書劍學從軍。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 40
    },
    {
        "title": "題崔公池亭舊遊",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "皎鏡方塘菡萏秋，此來重見採蓮舟。誰能不逐當年樂，",
            "簷前依舊青山色，盡日無人獨上樓。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 41
    },
    {
        "title": "回中作",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "蒼莽寒空遠色愁，嗚嗚戍角上高樓。吳姬怨思吹雙管，",
            "夜來霜重西風起，隴水無聲凍不流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 42
    },
    {
        "title": "西江上送漁父",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "卻逐嚴光向若耶，釣輪菱棹寄年華。三秋梅雨愁楓葉，",
            "白蘋風起樓船暮，江燕雙雙五兩斜。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 43
    },
    {
        "title": "經故秘書崔監揚州南塘舊居",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "昔年曾識范安成，松竹風姿鶴性情。西掖曙河橫漏響，",
            "千頃水流通故墅，至今留得謝公名。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 44
    },
    {
        "title": "七夕",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "鵲歸燕去兩悠悠，青瑣西南月似鉤。天上歲時星右轉，",
            "蘇小橫塘通桂楫，未應清淺隔牽牛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 45
    },
    {
        "title": "題韋籌博士草堂",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "玄晏先生已白頭，不隨鵷鷺狎群鷗。元卿謝免開三徑，",
            "滄浪未濯塵纓在，野水無情處處流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 46
    },
    {
        "title": "和友人題壁",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "沖尚猶來出範圍，肯將經世作風徽。三台位缺嚴陵臥，",
            "西州未有看棋暇，澗戶何由得掩扉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 47
    },
    {
        "title": "春日將欲東歸寄新及第苗紳先輩",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "幾年辛苦與君同，得喪悲歡儘是空。猶喜故人先折桂，",
            "知有杏園無路入，馬前惆悵滿枝紅。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 48
    },
    {
        "title": "經李征君故居",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "露濃煙重草萋萋，樹映闌幹柳拂堤。一院落花無客醉，",
            "惆悵羸驂往來慣，每經門巷亦長嘶。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 49
    },
    {
        "title": "送崔郎中赴幕",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "一別黔巫似斷弦，故交東去更淒然。心遊目送三千里，",
            "相思休話長安遠，江月隨人處處圓。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 50
    },
    {
        "title": "經舊遊",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "珠箔金鉤對彩橋，昔年於此見嬌嬈。香燈悵望飛瓊鬢，",
            "壞牆經雨蒼苔遍，拾得當時舊翠翹。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 51
    },
    {
        "title": "老君廟",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "紫氣氤氳捧半岩，蓮峰仙掌共巉巉。廟前晚色連寒水，",
            "自憐金骨無人識，知有飛龜在石函。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 52
    },
    {
        "title": "過五丈原",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "鐵馬雲雕久絕塵，柳陰高壓漢營春。天晴殺氣屯關右，",
            "象床錦帳無言語，從此譙周是老臣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 53
    },
    {
        "title": "和友人傷歌姬",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "月缺花殘莫愴然，花須終發月終圓。更能何事銷芳念，",
            "王孫莫學多情客，自古多情損少年。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 54
    },
    {
        "title": "山中與諸道友夜坐聞邊防不甯因示同志",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "龍砂鐵馬犯煙塵，跡近群鷗意倍親。風卷蓬根屯戊己，",
            "心許故人知此意，古來知者竟誰人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 55
    },
    {
        "title": "祕書省有賀監知章草題詩筆力遒健風尚高遠拂塵尋玩因有此作",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "越溪漁客賀知章，任達憐才愛酒狂。鸂鶒葦花隨釣艇，",
            "榮路脫身終自得，福庭回首莫相忘。出籠鸞鶴歸遼海，"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 56
    },
    {
        "title": "題裴晉公林亭",
        "author": "溫庭筠",
        "biography": "",
        "paragraphs": [
            "謝傅林亭暑氣微，山丘零落閟音徽。東山終為蒼生起，",
            "悠然到此忘情處，一日何妨有萬幾。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百七十八",
        "no#": 57
    }
]