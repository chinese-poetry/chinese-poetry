[
    {
        "title": "登蕪城",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "昔人登此地，丘壟已前悲。今日又非昔，春風能幾時。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 1
    },
    {
        "title": "滄浪峽",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "滄浪臨古道，道上石成塵。自有滄浪峽，誰為無事人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 2
    },
    {
        "title": "垓城",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "百戰未言非，孤軍驚夜圍。山河意氣盡，淚濕美人衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 3
    },
    {
        "title": "南陂遠望",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "閑門橫古塘，紅樹已驚霜。獨立望秋草，野人耕夕陽。",
            "孤煙起蝸舍，飛鷺下漁梁。唯有田家事，依依似故鄉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 4
    },
    {
        "title": "宿玉簫宮",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "塵飛不到空，露濕翠微宮。鶴影石橋月，簫聲松殿風。",
            "綠毛辭世女，白髮入壺翁。借問燒丹處，桃花幾遍紅。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 5
    },
    {
        "title": "晚眺徐州延福寺",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "杉風振旅塵，晚景藉芳茵。片水明在野，萬花深見人。",
            "靜依歸鶴思，遠惜舊山春。今日惜攜手，寄懷吟白蘋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 6
    },
    {
        "title": "宿甘棠館",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "塵跡入門盡，悄然江海心。水聲巫峽遠，山色洞庭深。",
            "風桂落寒子，嵐煙凝夕陰。前軒鶴歸處，蘿月思沉沉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 7
    },
    {
        "title": "和茅山高拾遺憶山中雜題五首：山泉",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "香味清機仙府回，縈紆亂石便流杯。",
            "春風莫泛桃花去，恐引凡人入洞來。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 8
    },
    {
        "title": "和茅山高拾遺憶山中雜題五首：巢鶴",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "千萬雲間丁令威，殷勤仙骨莫先飛。",
            "若逢茅氏傳消息，貞白先生不久歸。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 9
    },
    {
        "title": "和茅山高拾遺憶山中雜題五首：胡山",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "犬入五雲音信絕，鳳樓凝碧悄無聲。",
            "焚香古洞步虛夜，露濕松花空月明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 10
    },
    {
        "title": "和茅山高拾遺憶山中雜題五首：小樓",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "松杉風外亂山青，曲幾焚香對石屏。",
            "空憶去年春雨後，燕泥時汙太玄經。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 11
    },
    {
        "title": "和茅山高拾遺憶山中雜題五首：山鄰",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "石橋春澗已歸遲，夢入仙山山不知。",
            "柱史從來非俗吏，青牛道士莫相疑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 12
    },
    {
        "title": "送道士",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "泠然禦風客，與道自浮沉。黃鶴有歸語，白雲無忌心。",
            "藥爐經月淨，天路入壺深。從此分杯後，相思何處尋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 13
    },
    {
        "title": "宿山館",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "寂寞對衰草，地涼凝露華。蟬鳴月中樹，風落客前花。",
            "山館無宿伴，秋琴初別家。自憐千萬里，筆硯寄生涯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 14
    },
    {
        "title": "入浮石山",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "斜日出門去，殘花已過春。鳥聲穿葉遠，虎跡渡溪新。",
            "入洞幾時路，耕田何代人。自慚非避俗，不敢問迷津。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 15
    },
    {
        "title": "送人歸故園",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "遠節慘言別，況予心久違。從來憶家淚，今日送君歸。",
            "野路正風雪，還鄉猶布衣。裏中耕稼者，應笑讀書非。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 16
    },
    {
        "title": "得越中書",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "芳草離離思，悠悠春夢餘。池亭千里月，煙水一封書。",
            "詩想懷康樂，文應吊子胥。扁舟戀南越，豈獨為鱸魚。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 17
    },
    {
        "title": "秋墅",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "欲暮候樵者，望山空翠微。虹隨餘雨散，鴉帶夕陽歸。",
            "窮巷長秋草，孤村時擣衣。誰知多病客，寂寞掩柴扉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 18
    },
    {
        "title": "哭彭先生",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "穀口溪聲客自傷，那堪嗚咽吊殘陽。空階鶴戀丹青影，",
            "門人遠赴心喪夜，月滿千山舊草堂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 19
    },
    {
        "title": "和顧非熊先生題茅山處士閒居",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "歸耕地肺絕塵喧，匣裏青萍未報恩。濁酒自憐終日醉，",
            "唯有階前芳草色，年年惆悵憶王孫。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 20
    },
    {
        "title": "長安懷古",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "禍稔蕭牆終不知，生人力屈盡邊陲。赤龍已赴東方暗，",
            "秋風解怨扶蘇死，露泣煙愁紅樹枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 21
    },
    {
        "title": "過王右丞書堂二首",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "澄潭昔臥龍，章句世為宗。獨步聲名在，千岩水石空。",
            "野禽悲灌木，落日吊清風。後學攀遺址，秋山聞草蟲。",
            "萬樹影參差，石床藤半垂。螢光雖散草，鳥跡尚臨池。",
            "風雅傳今日，雲山想昔時。感深蘇屬國，千載五言詩。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 22
    },
    {
        "title": "孤雁",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "孤雁暮飛急，蕭蕭天地秋。關河正黃葉，消息斷青樓。",
            "湘渚煙波遠，驪山風雨愁。此時萬里道，魂夢繞滄洲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 23
    },
    {
        "title": "贈隱者",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "盡室居幽谷，亂山為四鄰。霧深知有術，窗靜似無人。",
            "鶴語松上月，花明雲裏春。生涯更何許，尊酒與垂綸。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 24
    },
    {
        "title": "早春懷薛公裕",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "幽獨自成愚，柴門日漸蕪。陸機初入洛，孫楚又游吳。",
            "失意怨楊柳，異鄉聞鷓鴣。相思複相望，春草滿南湖。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 25
    },
    {
        "title": "村月",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "月午籬南道，前村半隱林。田翁獨歸處，蕎麥露花深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 26
    },
    {
        "title": "早春",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "野樹花初發，空山獨見時。踟躕曆陽道，鄉思滿南枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 27
    },
    {
        "title": "送友人游吳",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "吳山青楚吟，草色異鄉心。一酌水邊酒，數聲花下琴。",
            "登樓舊國遠，探穴九疑深。更想逢秋節，那堪聞夜砧。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 28
    },
    {
        "title": "贈別",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "蘭澤傷秋色，臨風遠別期。東城草雖綠，南浦柳無枝。",
            "直道豈易枉，暗投誰不疑。因君問行役，有淚濕江蘺。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 29
    },
    {
        "title": "經故人舊居",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "萬里訪遺塵，鶯聲淚濕巾。古書無主散，廢宅與山鄰。",
            "宿草風悲夜，荒村月吊人。淒涼問殘柳，今日為誰春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 30
    },
    {
        "title": "隨邊使過五原",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "偶逐星車犯虜塵，故鄉常恐到無因。",
            "五原西去陽關廢，日漫平沙不見人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 31
    },
    {
        "title": "春懷寄秣陵知友",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "廬江城外柳堪攀，萬里行人尚未還。",
            "借問景陽臺下客，謝家誰更臥東山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 32
    },
    {
        "title": "宋州月夜感懷",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "雁池衰草露沾衣，河水東流萬事微。",
            "寂寞青陵臺上月，秋風滿樹鵲南飛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 33
    },
    {
        "title": "吳宮",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "荒台荊棘多，忠諫竟如何。細草迷宮巷，閑花誤綺羅。",
            "前溪徒自綠，子夜不聞歌。悵望清江暮，悠悠東去波。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 34
    },
    {
        "title": "春遊望仙谷",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "清無車馬塵，深洞百花春。雞犬疑沾藥，耕桑似避秦。",
            "登山采樵路，臨水浣紗人。若得心無事，移家便卜鄰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 35
    },
    {
        "title": "聖女祠",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "石屏苔色涼，流水繞祠堂。巢鵲疑天漢，潭花似鏡妝。",
            "神來雲雨合，神去蕙蘭香。不復聞雙佩，山門空夕陽。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 36
    },
    {
        "title": "宿范水",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "行人倦游宦，秋草宿湖邊。露濕芙蓉渡，月明漁網船。",
            "寒機深竹裏，遠浪到門前。何處思鄉甚，歌聲聞採蓮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 37
    },
    {
        "title": "送顧陶校書歸錢塘",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "清苦月偏知，南歸瘦馬遲。橐輕緣換酒，發白為吟詩。",
            "水色西陵渡，松聲伍相祠。聖朝思直諫，不是掛冠時。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 38
    },
    {
        "title": "題雲陽高少府衙齋",
        "author": "儲嗣宗",
        "biography": "",
        "paragraphs": [
            "大隱能兼濟，軒窗逐勝開。遠含雲水思，深得棟樑材。",
            "吏散山逾靜，庭閑鳥自來。更憐幽砌色，秋雨長莓苔。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五百九十四",
        "no#": 39
    }
]