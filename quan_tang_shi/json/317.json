[
    {
        "title": "送崔判官使太原",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "勞君車馬此逡巡，我與劉君本世親。兩地山河分節制，",
            "報主由來須盡敵，相期萬里寶刀新。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 1
    },
    {
        "title": "幕中諸公有觀獵之作因繼之",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "刀州城北劍山東，甲士屯雲騎散風。旌旆遍張林嶺動，",
            "為報府中諸從事，燕然未勒莫論功。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 2
    },
    {
        "title": "同幕中諸公送李侍禦歸朝",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "昔年專席奉清朝，今日持書即舊僚。珠履會中簫管思，",
            "歲月不堪相送盡，頹顏更被別離凋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 3
    },
    {
        "title": "送張六諫議歸朝",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "詔書前日下丹霄，頭戴儒冠脫皂貂。笛怨柳營煙漠漠，",
            "歸去朝端如有問，玉關門外老班超。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 4
    },
    {
        "title": "酬嚴司空荊南見寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "金貂再領三公府，玉帳連封萬戶侯。簾卷青山巫峽曉，",
            "白雪調高歌不得，美人南國翠蛾愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 5
    },
    {
        "title": "南徐別業早春有懷",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "生涯擾擾竟何成，自愛深居隱姓名。遠雁臨空翻夕照，",
            "虛度年華不相見，離腸懷土並關情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 6
    },
    {
        "title": "摩訶池宴",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "摩訶池上春光早，愛水看花日日來。穠李雪開歌扇掩，",
            "晝短欲將清夜繼，西園自有月裴回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 7
    },
    {
        "title": "至櫟陽崇道寺聞嚴十少府趨侍",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "雲連萬木夕沉沉，草色泉聲古院深。聞說羊車趨盛府，",
            "惆悵送君身未達，不堪搖落聽秋砧。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 8
    },
    {
        "title": "春暮郊居寄朱舍人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "幽深不讓子真居，度日閑眠世事疏。春水滿池新雨霽，",
            "回首知音青瑣闥，何時一為薦相如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 9
    },
    {
        "title": "送溫況游蜀",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "遊人西去客三巴，身逐孤蓬不定家。山近峨眉飛暮雨，",
            "應到嚴君開卦處，將餘一為問生涯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 10
    },
    {
        "title": "崔敷歎春物將謝恨不同覽時余方為事牽束及往尋不遇題之留贈",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "九陌遲遲麗景斜，禁街西訪隱淪賒。門依高柳空飛絮，",
            "殘陽寂寞東城去，惆悵春風落盡花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 11
    },
    {
        "title": "秋燈對雨寄史近崔積",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "坐聽宮城傳晚漏，起看衰葉下寒枝。空庭綠草結離念，",
            "相逢莫厭尊前醉，春去秋來自不知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 12
    },
    {
        "title": "春題龍門香山寺",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "眾香天上梵仙宮，鐘磬寥寥半碧空。清景乍開松嶺月，",
            "欲盡出尋那可得，三千世界本無窮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 13
    },
    {
        "title": "酬陸三與鄒十八侍禦",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "城分流水郭連山，拂露開懷一解顏。令尹關中仙史會，",
            "莫恨殷勤留此地，東崖桂樹昔同攀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 14
    },
    {
        "title": "酬談校書長安秋夜對月寄諸故舊",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "故園千里渺遐情，黃葉蕭條白露生。驚鵲繞枝風滿幌，",
            "莫怪孔融悲歲序，五侯門館重婁卿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 15
    },
    {
        "title": "送田三端公還鄂州",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "孤雲迢遞戀滄洲，勸酒梨花對白頭。南陌送歸車騎合，",
            "君去庾公應借問，馳心千里大江流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 16
    },
    {
        "title": "送李秀才赴滑州詣大夫舅",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "陌頭車馬去翩翩，白麵懷書美少年。東武揚公姻婭重，",
            "知己滿朝留不住，貴臣河上擁旌旃。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 17
    },
    {
        "title": "秋日書懷",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "金貂玉鉉奉君恩，夜漏晨鐘老掖垣。參決萬機空有愧，",
            "倦鳥不知歸去日，青蕪白露滿郊園。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 18
    },
    {
        "title": "南昌灘",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "渠江明淨峽逶迤，船到名灘拽𥮘遲。櫓窡動搖妨作夢，",
            "物色可憐心莫限，此行都是獨行時。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 19
    },
    {
        "title": "奉和聖制豐年多慶九日示懷",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "令節寰宇泰，神都佳氣濃。賡歌禹功盛，擊壤堯年豐。",
            "九奏碧霄裏，千官皇澤中。南山澄凝黛，曲水清涵空。",
            "金玉美王度，歡康謠國風。睿文垂日月，永與天無窮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 20
    },
    {
        "title": "夏日陪馮許二侍郎與嚴祕書遊昊天觀覽舊題寄同里楊華州中丞",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "三伏草木變，九城車馬煩。碧霄回騎射，丹洞入桃源。",
            "台殿雲浮棟，緌纓鶴在軒。莫將真破妄，聊用靜持喧。",
            "石甃古苔冷，水筠涼簟翻。黃公壚下歎，旌旆國東門。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 21
    },
    {
        "title": "奉和聖制重陽日即事",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "玉燭降寒露，我皇歌古風。重陽德澤展，萬國歡娛同。",
            "綺陌擁行騎，香塵凝曉空。神都自藹藹，佳氣助蔥蔥。",
            "律呂陰陽暢，景光天地通。徒然被鴻霈，無以報玄功。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 22
    },
    {
        "title": "秋日台中寄懷簡諸僚",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "憲府日多事，秋光照碧林。幹雲岩翠合，布石地苔深。",
            "憂悔耿遐抱，塵埃緇素襟。物情牽局促，友道曠招尋。",
            "頹節風霜變，流年芳景侵。池荷足幽氣，煙竹又繁陰。",
            "簪組赤墀戀，池魚滄海心。滌煩滯幽賞，永度瑤華音。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 23
    },
    {
        "title": "奉酬淮南中書相公見寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "揚州隋故都，竹使漢名儒。翊聖恩華異，持衡節制殊。",
            "朝廷連受脤，台座接籲謨。金玉裁王度，丹書奉帝俞。",
            "九重辭象魏，千里握兵符。鐵馬秋臨塞，虹旌夜渡瀘。",
            "江長梅笛怨，天遠桂輪孤。浩歎煙霜曉，芳期蘭蕙蕪。",
            "雅言書一劄，賓海雁東隅。歲月奔波盡，音徽霧雨濡。",
            "蜀江分井絡，錦浪入淮湖。獨抱相思恨，關山不可逾。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 24
    },
    {
        "title": "甫構西亭偶題因呈監軍及幕中諸公",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "瀛海無因泛，昆丘豈易尋。數峰聊在目，一境暫清心。",
            "悅彼松柏性，愛茲桃李陰。列芳憑有土，叢幹聚成林。",
            "信矣子牟戀，歸歟尼父吟。暗香蘭露滴，空翠蕙樓深。",
            "負鼎位嘗忝，荷戈年屢侵。百城煩鞅掌，九仞喜嶇嶔。",
            "巴漢溯沿楫，岷峨千萬岑。恩偏不敢去，範蠡畏熔金。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 25
    },
    {
        "title": "和楊弘微春日曲江南望",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "遲景靄悠悠，傷春南陌頭。暄風一澹蕩，遐思幾殷優。",
            "龍去空仙沼，鸞飛掩妓樓。芳菲餘雨露，冠蓋舊公侯。",
            "朱戟千門閉，黃鸝百囀愁。煙蒙宮樹晚，花咽石泉流。",
            "寒谷律潛應，中林蘭自幽。商山將避漢，晉室正藩周。",
            "黍稷聞興歎，瓊瑤畏見投。君心即吾事，微向在滄洲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 26
    },
    {
        "title": "長安秋夜懷陳京昆季",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "鐘鼓九衢絕，出門千里同。遠情高枕夜，秋思北窗空。",
            "靜見煙凝燭，閑聽葉墜桐。玉壺思洞徹，瓊樹憶蔥籠。",
            "螢影疏簾外，鴻聲暗雨中。羈愁難會面，懶慢責微躬。",
            "甲乙科攀桂，圖書閣踐蓬。一瓢非可樂，六翮未因風。",
            "寥落悲秋盡，蹉跎惜歲窮。明朝不相見，流淚菊花叢。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 27
    },
    {
        "title": "冬日，漢江南行將赴夏口，途次江陵界，寄裴尚書",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "五部擁雙旌，南依墨客卿。關山迥梁甸，波浪接湓城。",
            "煙景迷時候，雲帆渺去程。蛤珠馮月吐，蘆雁觸羅驚。",
            "浦樹凝寒晦，江天湛鏡清。賞心隨處愜，壯志逐年輕。",
            "舟楫不可駐，提封如任情。向方曾指路，射策許言兵。",
            "蘭渚歇芳意，菱歌非應聲。元戎武昌守，羊祜幸連營。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 28
    },
    {
        "title": "奉酬中書李相公早朝于中書候傳點，偶書所懷",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "寥落曙鐘斷，微明煙月沉。翠霞仙仗合，清漏掖垣深。",
            "北極星遙拱，南山闕迥臨。蘭釭竟曉焰，琪樹欲秋陰。",
            "霄漢慚聯步，貂蟬愧並簪。德容溫比玉，王度式如金。",
            "魚水千年運，簫韶九奏音。代天驚度日，擲地喜開襟。",
            "文武時方泰，唐虞道可尋。忝陪申及甫，清淨奉堯心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 29
    },
    {
        "title": "甲午歲相國李公有北園寄贈之作吟玩歷時屢促酬答機務不暇未及報章今古遽分電波增感留墓劍而心許感鄰笛而意傷寓哀冥寞以廣遺韻云",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "機事勞西掖，幽懷寄北園。鶴巢深更靜，蟬噪斷猶喧。",
            "仙醞百花馥，豔歌雙袖翻。碧雲詩變雅，皇澤葉流根。",
            "未報雕龍贈，俄傷淚劍痕。佳城關白日，哀挽向青門。",
            "禮命公台重，煙霜隴樹繁。天高不可問，空使輔星昏。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 30
    },
    {
        "title": "和楊三舍人晚秋與崔二舍人張祕監苖考功同遊昊天觀時中書寓直不得陪隨因追往年曾與舊僚聯遊此觀紀題在壁已有淪亡書事感懷輒以呈寄兼呈東省三給事之作楊君見徵鄙詞因以繼和",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "瑤圃高秋會，金閨奉詔辰。朱輪天上客，白石洞中人。",
            "佩響泉聲雜，朝衣羽服親。九重青瑣閉，三秀紫芝新。",
            "化藥秦方士，偷桃漢侍臣。玉笙王子駕，遼鶴令威身。",
            "歎逝頹波速，緘詞麗曲春。重將淒恨意，苔壁問遺塵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 31
    },
    {
        "title": "酬李十一尚書西亭暇日書懷見寄十二韻之作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "鼎鉉昔雲忝，西南分主憂。煙塵開僰道，旌節護蠻陬。",
            "任重功無立，力微恩未酬。據鞍慚齒發，責帥懼春秋。",
            "高德聞鄭履，儉居稱晏裘。三刀君入夢，九折我回輈。",
            "時景屢遷易，茲言期退休。方追故山事，豈謂臺階留。",
            "遐抱清淨理，眷言蘭杜幽。一緘瓊玖贈，萬里別離愁。",
            "巴嶺雲外沒，蜀江天際流。懷賢耿遙思，相望鳳池頭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 32
    },
    {
        "title": "秋懷奉寄朱補闕",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "上苑繁霜降，騷人起恨初。白雲深陋巷，衰草遍閒居。",
            "暮色秋煙重，寒聲牖葉虛。潘生秋思苦，陶令世情疏。",
            "已制歸田賦，猶陳諫獵書。不知青瑣客，投分竟何如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 33
    },
    {
        "title": "奉酬中書相公至日圜丘行事合於中書宿齋移止於集賢院敘情見寄之什",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "郊廟祗嚴祀，齋莊覿上玄。別開金虎觀，不離紫微天。",
            "樹古長楊接，池清太液連。仲山方補袞，文舉自傷年。",
            "風溢銅壺漏，香凝綺閣煙。仍聞白雪唱，流詠滿鶤弦。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 34
    },
    {
        "title": "途次",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "去國策羸馬，勞歌行路難。地崇秦制險，人樂漢恩寬。",
            "禦沼澄泉碧，宮梨佛露丹。鼎成仙馭遠，龍化宿雲殘。",
            "不問三苗寵，誰陪萬國歡。至今松桂色，長助玉樓寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 35
    },
    {
        "title": "題故蔡國公主九華觀上池院",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "朱門臨九衢，雲木藹仙居。曲沼天波接，層台鳳舞餘。",
            "曙煙深碧筱，香露濕紅蕖。瑤瑟含風韻，紗窗積翠虛。",
            "秦樓今寂寞，真界竟何如。不與蓬瀛異，迢迢遠玉除。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 36
    },
    {
        "title": "酬嚴維秋夜見寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "遙夜思悠悠，聞鐘遠夢休。亂林螢燭暗，零露竹風秋。",
            "啟戶雲歸棟，褰簾月上鉤。昭明逢聖代，羈旅別滄洲。",
            "騎省潘郎思，衡闈宋玉愁。神仙慚李郭，詞賦謝曹劉。",
            "松柏應無變，瓊瑤不可酬。誰堪此時景，寂寞下高樓。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 37
    },
    {
        "title": "途次近蜀驛蒙恩賜寶刀及飛龍廄馬使還奉寄中書李鄭二公",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "草草事行役，遲遲違故關。碧幃遙隱霧，紅旆漸依山。",
            "感激酬恩淚，星霜去國顏。捧刀金錫字，歸馬玉連環。",
            "威鳳翔雙闕，征夫縱百蠻。應憐宣室召，溫樹不同攀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 38
    },
    {
        "title": "歸燕",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "春色遍芳菲，閑簷雙燕歸。還同舊侶至，來繞故巢飛。",
            "敢望煙霄達，多慚羽翮微。銜泥傍金砌，拾蕊到荊扉。",
            "雲海經時別，雕梁長日依。主人能一顧，轉盻自光輝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 39
    },
    {
        "title": "送許著作分司東都",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "瑤瑟激淒響，征鴻翻夕陽。署分刊竹簡，書蠹護芸香。",
            "馬色關城曉，蟬聲驛路長。石渠榮正禮，蘭室重元方。",
            "不作經年別，離魂亦暫傷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 40
    },
    {
        "title": "聞相公三兄小園置宴以元衡寓直因寄上兼呈中書三兄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "休沐限中禁，家山傳勝遊。露寒潘省夜，木落庾園秋。",
            "蘭菊回幽步，壺觴洽舊儔。位高天祿閣，詞異畔牢愁。",
            "孤思琴先覺，馳暉水競流。明朝不相見，清祀在圜丘。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 41
    },
    {
        "title": "酬陸員外歙州許員外郢州二使君",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "吳洲雲海接，楚驛夢林長。符節分憂重，鵷鴻去路翔。",
            "豔歌愁翠黛，寶瑟韻清商。洲草遙池合，春風曉旆張。",
            "晉臣多樂廣，漢主識馮唐。不作經年別，離魂亦未傷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 42
    },
    {
        "title": "漸至涪州先寄王使君",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "治教通夷俗，均輸問大田。江分巴字水，樹入夜郎煙。",
            "毒霧含秋氣，陰岩蔽曙天。路難空計日，身老不由年。",
            "將命寧知遠，歸心詎可傳。星郎複何事，出守五溪邊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 43
    },
    {
        "title": "石州城",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "丈夫心愛橫行，報國知嫌命輕。樓蘭徑百戰，"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 44
    },
    {
        "title": "寒食下第",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "柳掛九衢絲，花飄萬家雪。如何憔悴人，對此芳菲節。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 45
    },
    {
        "title": "途中即事",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "南征複北還，擾擾百年間。自笑紅塵裏，生涯不暫閑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 46
    },
    {
        "title": "春日偶作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "縱橫桃李枝，淡蕩春風吹。美人歌白苧，萬恨在蛾眉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 47
    },
    {
        "title": "夏夜作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "夜久喧暫息，池台惟月明。無因駐清景，日出事還生。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 48
    },
    {
        "title": "左掖梨花",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "巧笑解迎人，晴雪香堪惜。隨風蝶影翻，誤點朝衣赤。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 49
    },
    {
        "title": "同陳六侍禦寒食游禪定藏山上人院",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "年少輕行樂，東城南陌頭。與君寂寞意，共作草堂遊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 50
    },
    {
        "title": "贈佳人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "步搖金翠玉搔頭，傾國傾城勝莫愁。",
            "若逞仙姿遊洛浦，定知神女謝風流。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 51
    },
    {
        "title": "休暇日，中書相公致齋禁省，因以寄贈",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "嘗聞聖主得賢臣，三接能令四海春。",
            "月滿禁垣齋沐夜，清吟屬和更何人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 52
    },
    {
        "title": "春興",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "楊柳陰陰細雨晴，殘花落盡見流鶯。",
            "春風一夜吹香夢，夢逐春風到洛城。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 53
    },
    {
        "title": "酬裴起居西亭留題",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "豔歌能起關山恨，紅燭偏凝寒塞情。",
            "況是池塘風雨夜，不堪絲管盡離聲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 54
    },
    {
        "title": "送張侍禦赴京",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "江南煙雨塞鴻飛，西府文章謝掾歸。",
            "相送汀州蘭棹晚，菱歌一曲淚沾衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 55
    },
    {
        "title": "鄂渚送友",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "雲帆淼淼巴陵渡，煙樹蒼蒼故郢城。",
            "江上梅花無數落，送君南浦不勝情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 56
    },
    {
        "title": "送裴戡行軍",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "珠履三千醉不歡，玉人猶苦夜冰寒。",
            "送君偏有無言淚，天下關山行路難。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 57
    },
    {
        "title": "宿青陽驛",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "空山搖落三秋暮，螢過疏簾月露團。",
            "寂寞銀燈愁不寐，蕭蕭風竹夜窗寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 58
    },
    {
        "title": "送崔舍人起居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "赤墀同拜紫泥封，駟牡連征侍九重。",
            "惟有白須張司馬，不言名利尚相從。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 59
    },
    {
        "title": "路岐重賦",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "芳郊欲別闌幹淚，故國難期聚散雲。",
            "分手更逢江驛暮，馬嘶猿叫不堪聞。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 60
    },
    {
        "title": "重送盧三十一起居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "相如擁傳有光輝，何事闌幹淚濕衣。",
            "舊府東山餘妓在，重將歌舞送君歸。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 61
    },
    {
        "title": "送張諫議",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "漢庭從事五人來，回首疆場獨未回。",
            "今日送君魂斷處，寒雲寥落數株梅。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 62
    },
    {
        "title": "同諸公夜宴監軍玩花之作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "五侯門館百花繁，紅燭搖風白雪翻。",
            "不似鳳凰池畔見，飄揚今隔上林園。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 63
    },
    {
        "title": "郊居寓目偶題",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "晨趨禁掖暮郊園，松桂蒼蒼煙露繁。",
            "明月上時群動息，雪峰高處正當軒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 64
    },
    {
        "title": "題嘉陵驛",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "悠悠風旆繞山川，山驛空蒙雨似煙。",
            "路半嘉陵頭已白，蜀門西上更青天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 65
    },
    {
        "title": "送柳郎中裴起居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "望鄉臺上秦人在，學射山中杜魄哀。",
            "落日河橋千騎別，春風寂寞旆旌回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 66
    },
    {
        "title": "秋日出遊偶作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "黃花丹葉滿江城，暫愛江頭風景清。",
            "閒步欲舒山野性，貔貅不許獨行人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 67
    },
    {
        "title": "夏夜餞裴行軍赴朝命",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "三年同看錦城花，銀燭連宵照綺霞。",
            "報國從來先意氣，臨岐不用重咨嗟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 68
    },
    {
        "title": "摩訶池送李侍禦之鳳翔",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "柳暗花明池上山，高樓歌酒換離顏。",
            "他時欲寄相思字，何處黃雲是隴間。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 69
    },
    {
        "title": "送魏正則擢第歸江陵",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "商山路接玉山深，古木蒼然盡合陰。",
            "會府登筵君最少，江城秋至肯驚心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 70
    },
    {
        "title": "登闔閭古城",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "登高望遠自傷情，柳發花開映古城。",
            "全盛已隨流水去，黃鸝空囀舊春聲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 71
    },
    {
        "title": "尋三藏上人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "北風吹雪暮蕭蕭，問法尋僧上界遙。",
            "臨水手持筇竹杖，逢君不語指芭蕉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 72
    },
    {
        "title": "山居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "身依泉壑將時背，路入煙蘿得地深。",
            "終歲不知城郭事，手栽林竹盡成陰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 73
    },
    {
        "title": "長安賊中寄題江南所居茱萸樹",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "手種茱萸舊井傍，幾回春露又秋霜。",
            "今來獨向秦中見，攀折無時不斷腸。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 74
    },
    {
        "title": "春齋夜雨憶郭通微",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "桃源在在阻風塵，世事悠悠又遇春。",
            "雨滴閑階清夜久，焚香偏憶白雲人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 75
    },
    {
        "title": "送嚴秀才",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "灞滻別離腸已斷，江山迢遞信仍稀。",
            "送君偏下臨岐淚，家在南州身未歸。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 76
    },
    {
        "title": "春日酬熊執易南亭花發見贈",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "千株桃杏參差發，想見花時人卻愁。",
            "曾忝陸機琴酒會，春亭惟願一淹留。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 77
    },
    {
        "title": "中春亭雪夜寄西鄰韓李二舍人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "廣庭飛雪對愁人，寒谷由來不悟春。",
            "卻笑山陰乘興夜，何如今日戴家鄰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 78
    },
    {
        "title": "立秋日與陸華原于縣界南館送鄒十八",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "風入昭陽池館秋，片雲孤鶴兩難留。",
            "明朝獨向青山郭，唯有蟬聲催白頭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 79
    },
    {
        "title": "酬韋胄曹登天長寺上方見寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "青門幾度沾襟淚，並在東林雪外峰。",
            "今日重煩相憶處，春光知繞鳳池濃。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 80
    },
    {
        "title": "陌上暮春",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "青青南陌柳如絲，柳色鶯聲晚日遲。",
            "何處最傷遊客思，春風三月落花時。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 81
    },
    {
        "title": "春日偶作",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "飛花寂寂燕雙雙，南客衡門對楚江。",
            "惆悵管弦何處發，春風吹到讀書窗。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 82
    },
    {
        "title": "春暮寄杜嘉興昆弟",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "柳色千家與萬家，輕風細雨落殘花。",
            "數枝瓊玉無由見，空掩柴扉度歲華。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 83
    },
    {
        "title": "渡淮",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "暮濤凝雪長淮水，細雨飛梅五月天。",
            "行子不須愁夜泊，綠楊多處有人煙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 84
    },
    {
        "title": "與崔十五同訪裴校書不遇",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "梨花落盡柳花時，庭樹流鶯日過遲。",
            "幾度相思不相見，春風何處有佳期。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 85
    },
    {
        "title": "夏日寄陸三達陸四逢並王念八仲周",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "士衡兄弟舊齊名，還似當年在洛城。",
            "聞說重門方隱相，古槐高柳夏陰清。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 86
    },
    {
        "title": "秋原寓目",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "木落風高天宇開，秋原一望思悠哉。",
            "邊城今少射雕騎，連雁嗷嗷何處來。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 87
    },
    {
        "title": "贈歌人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "林鶯一哢四時春，蟬翼羅衣白玉人。",
            "曾逐使君歌舞地，清聲長嘯翠眉顰。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 88
    },
    {
        "title": "同苗郎中送嚴侍禦赴黔中，因訪仙源之事",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "武陵源在朗江東，流水飛花仙洞中。",
            "莫問阮郎千古事，綠楊深處翠霞空。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 89
    },
    {
        "title": "使次盤豆驛望永樂縣",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "山川不記何年別，城郭應非昔所經。",
            "欲駐征車終日望，天河雲雨晦冥冥。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 90
    },
    {
        "title": "緱山道中口號",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "秋山寂寂秋水清，寒郊木葉飛無聲。",
            "王子白雲仙去久，洛濱行路夜吹笙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 91
    },
    {
        "title": "歲暮送舍人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "邊城歲暮望鄉關，身逐戎旌未得還。",
            "欲別臨岐無限淚，故園花發寄君攀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 92
    },
    {
        "title": "寓興呈崔員外諸公",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "三月楊花飛滿空，飄颻十裏雪如風。",
            "不知何處香醪熟，願醉佳園芳樹中。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 93
    },
    {
        "title": "單于曉角",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "胡兒吹角漢城頭，月皎霜寒大漠秋。",
            "三奏未終天便曉，何人不起望鄉愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 94
    },
    {
        "title": "汴河聞笳",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "何處金笳月裏悲，悠悠邊客夢先知。",
            "單于城下關山曲，今日中原總解吹。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 95
    },
    {
        "title": "塞上春懷",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "東風河外五城喧，南客征袍滿淚痕。",
            "愁至獨登高處望，藹然雲樹重傷魂。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 96
    },
    {
        "title": "塞外月夜寄荊南熊侍禦",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "南依劉表北劉琨，征戰年年簫鼓喧。",
            "雲雨一乖千萬里，長城秋月洞庭猿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 97
    },
    {
        "title": "單于罷戰卻歸題善陽館",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "單于南去善陽關，身逐歸雲到處閑。",
            "曾是五年蓮府客，每聞胡虜哭陰山。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 98
    },
    {
        "title": "韋常侍以賓客致仕，同諸公題壁",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "孤雲永日自徘徊，岩館蒼蒼遍綠苔。",
            "望苑忽驚新詔下，彩鸞歸處玉籠開。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 99
    },
    {
        "title": "學仙難",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "玉殿笙歌漢帝愁，鸞龍儼駕望瀛洲。",
            "黃金化盡方士死，青天欲上無緣由。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 100
    },
    {
        "title": "唐昌觀玉蕊花",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "琪樹芊芊玉蕊新，洞宮長閉彩霞春。",
            "日暮落英鋪地雪，獻花應過九天人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 101
    },
    {
        "title": "春曉聞鶯",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "寥寥蘭台曉夢驚，綠林殘月思孤鶯。",
            "猶疑蜀魄千年恨，化作冤禽萬囀聲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 102
    },
    {
        "title": "聞嚴秘書與正字及諸客夜會因寄",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "衡門寥落歲陰窮，露濕莓苔葉厭風。",
            "聞道今宵阮家會，竹林明月七人同。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 103
    },
    {
        "title": "戲贈韓二秀才",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "名高折桂方年少，心苦為文命未通。",
            "聞說東堂今有待，飛鳴何處及春風。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 104
    },
    {
        "title": "聞王仲周所居牡丹花發，因戲贈",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "聞說庭花發暮春，長安才子看須頻。",
            "花開花落無人見，借問何人是主人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 105
    },
    {
        "title": "酬王十八見招",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "王昌家直在城東，落盡庭花昨夜風。",
            "高興不辭千日醉，隨君走馬向新豐。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 106
    },
    {
        "title": "贈道者",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "麻衣如雪一枝梅，笑掩微妝入夢來。",
            "若到越溪逢越女，紅蓮池裏白蓮開。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 107
    },
    {
        "title": "重送白將軍",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "紅燭芳筵惜夜分，歌樓管咽思難聞。",
            "早知怨別人間世，不下青山老白雲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 108
    },
    {
        "title": "和李丞題李將軍林園",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "落英飄蕊雪紛紛，啼鳥如悲霍冠軍。",
            "逝水不回弦管絕，玉樓迢遞鎖浮雲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 109
    },
    {
        "title": "同張惟送霍總",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "春風簫管怨津樓，三奏行人醉不留。",
            "別後相思江上岸，落花飛處杜鵑愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 110
    },
    {
        "title": "贈別崔起居",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "三十年前會府同，紅顏銷盡兩成翁。",
            "別淚共將何處灑，錦江南渡足春風。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 111
    },
    {
        "title": "春日偶題",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "山川百戰古刀州，龍節來分聖主憂。",
            "靜守化條無一事，春風獨上望京樓。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 112
    },
    {
        "title": "聽歌",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "月上重樓絲管秋，佳人夜唱古梁州。",
            "滿堂誰是知音者，不惜千金與莫愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 113
    },
    {
        "title": "酬韋胄曹",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "相逢異縣蹉跎意，無複少年容易歡。",
            "桃李美人攀折盡，何如松柏四時寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 114
    },
    {
        "title": "同幕府夜宴惜花",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "芳草落花明月榭，朝雲暮雨錦城春。",
            "莫愁紅豔風前散，自有青蛾鏡裏人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 115
    },
    {
        "title": "代佳人贈張郎中",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "洛陽佳麗本神仙，冰雪顏容桃李年。",
            "心愛阮郎留不住，獨將珠淚濕紅鉛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 116
    },
    {
        "title": "餞裴行軍赴朝命",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "來時聖主假光輝，心恃朝恩計日歸。",
            "誰料忽成雲雨別，獨將邊淚灑戎衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 117
    },
    {
        "title": "秋日經潼關感寓",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "昔年曾逐漢征東，三授兵符百戰中。",
            "力保山河嗟下世，秋風牢落故營空。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 118
    },
    {
        "title": "贈歌人",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "仙歌靜轉玉簫催，疑是流鶯禁苑來。",
            "他日相思夢巫峽，莫教雲雨晦陽臺。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 119
    },
    {
        "title": "見郭侍郎題壁",
        "author": "武元衡",
        "biography": "",
        "paragraphs": [
            "萬里楓江偶問程，青苔壁上故人名。",
            "悠悠身世限南北，一別十年空複情。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百一十七",
        "no#": 120
    }
]
