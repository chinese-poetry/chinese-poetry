[
    {
        "title": "送春詞",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "昨來樓上迎春處，今日登樓又送歸。蘭蕊殘妝含露泣，",
            "萬古至今同此恨，無如一醉盡忘機。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 1
    },
    {
        "title": "送李尚書鎮滑州",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "南徐報政入文昌，東郡須才別建章。視草名高同蜀客，",
            "自古相門還出相，如今人望在岩廊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 2
    },
    {
        "title": "送渾大夫赴豐州",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "鳳銜新詔降恩華，又見旌旗出渾家。故吏來辭辛屬國，",
            "其奈明年好春日，無人喚看牡丹花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 3
    },
    {
        "title": "送源中丞充新羅冊立使",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "相門才子稱華簪，持節東行捧德音。身帶霜威辭鳳闕，",
            "想見扶桑受恩處，一時西拜盡傾心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 4
    },
    {
        "title": "送王司馬之陝州",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "暫輟清齋出太常，空攜詩卷赴甘棠。府公既有朝中舊，",
            "兩京大道多遊客，每遇詞人戰一場。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 5
    },
    {
        "title": "洛中送楊處厚入關便游蜀",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "洛陽秋日正淒淒，君去西秦更向西。舊學三冬今轉富，",
            "早識臥龍應有分，不妨從此躡丹梯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 6
    },
    {
        "title": "送周使君罷渝州歸郢州別墅",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "君思郢上吟歸去，故自渝南擲郡章。野戍岸邊留畫舸，",
            "只恐鳴騶催上道，不容待得晚菘嘗。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 7
    },
    {
        "title": "奉送浙西李僕射相公赴鎮",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "建節東行是舊遊，歡聲喜氣滿吳州。郡人重得黃丞相，",
            "自憐不識平津閣，遙望旌旗汝水頭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 8
    },
    {
        "title": "重送浙西李相公頃廉問江南已經七載後歷滑臺劍南兩鎮遂入相今復領舊地新加旌旄",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "江北萬人看玉節，江南千騎引金鐃。鳳從池上游滄海，",
            "碧雞白馬回翔久，卻憶朱方是樂郊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 9
    },
    {
        "title": "送前進士蔡京赴學究科",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "耳聞戰鼓帶經鋤，振發聲名自裏閭。已是世間能賦客，",
            "幸遇天官舊丞相，知君無翼上空虛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 10
    },
    {
        "title": "送唐舍人出鎮閩中",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "暫辭鴛鷺出蓬瀛，忽擁貔貅鎮粵城。閩嶺夏雲迎皂蓋，",
            "了卻人間婚嫁事，複歸朝右作公卿。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 11
    },
    {
        "title": "送李中丞赴楚州",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "緹騎朱旗入楚城，士林皆賀振家聲。兒童但喜迎賓守，",
            "憶君初得昆山玉，同向揚州攜手行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 12
    },
    {
        "title": "奉送李戶部侍郎自河南尹再除本官歸闕",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "昔年內署振雄詞，今日東都結去思。宮女猶傳洞簫賦，",
            "想到金閨待通籍，一時驚喜見風儀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 13
    },
    {
        "title": "送蘄州李郎中赴任",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "楚關蘄水路非賒，東望雲山日夕佳。薤葉照人呈夏簟，",
            "北地交親長引領，早將玄鬢到京華。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 14
    },
    {
        "title": "送國子令狐博士赴興元覲省",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "相門才子高陽族，學省清資五品官。諫院過時榮棣萼，",
            "伯仲到家人盡賀，柳營蓮府遞相歡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 15
    },
    {
        "title": "送李二十九兄員外赴邠甯使幕",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "家襲韋平身業文，素風清白至今貧。南宮通籍新郎吏，",
            "鼎門為別霜天曉，剩把離觴三五巡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 16
    },
    {
        "title": "送分司陳郎中只召直史館重修三聖實錄",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "蟬鳴官樹引行車，言自成周赴玉除。遠取南朝貴公子，",
            "若問舊人劉子政，如今白首在南徐。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 17
    },
    {
        "title": "送慧則法師歸上都因呈廣宣上人",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "昨日東林看講時，都人象馬蹋琉璃。雪山童子應前世，",
            "休公久別如相問，楚客逢秋心更悲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 18
    },
    {
        "title": "送義舟師卻還黔南",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "黔江秋水浸雲霓，獨泛慈航路不迷。猿狖窺齋林葉動，",
            "常說摩圍似靈鷲，卻將山屐上丹梯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 19
    },
    {
        "title": "送景玄師東歸",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "東林寺裏一沙彌，心愛當時才子詩。山下偶隨流水出，",
            "想到舊房拋錫杖，小松應有過簷枝。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 20
    },
    {
        "title": "漢壽城春望",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "漢壽城邊野草春，荒祠古墓對荊榛。田中牧豎燒芻狗，",
            "不知何日東瀛變，此地還成要路津。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 21
    },
    {
        "title": "荊門道懷古",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "南國山川舊帝畿，宋台梁館尚依稀。馬嘶古道行人歇，",
            "徒使詞臣庾開府，咸陽終日苦思歸。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 22
    },
    {
        "title": "朗州竇員外見示與澧州元郎中郡齋贈答長句二篇因以繼和",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "鴛鷺差池出建章，彩旗朱戶蔚相望。新恩共理犬牙地，",
            "應憐一罷金閨籍，枉渚逢春十度傷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 23
    },
    {
        "title": "早春對雪奉寄澧州元郎中",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "新賜魚書墨未幹，賢人暫出遠人安。朝驅旌旆行時令，",
            "甯知楚客思公子，北望長吟澧有蘭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 24
    },
    {
        "title": "竇朗州見示與澧州元郎中早秋贈答命同作",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "鄰境諸侯同舍郎，芷江蘭浦恨無梁。秋風門外旌旗動，"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 25
    },
    {
        "title": "松滋渡望峽中",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "渡頭輕雨灑寒梅，雲際溶溶雪水來。夢渚草長迷楚望，",
            "十二碧峰何處所，永安宮外是荒台。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 26
    },
    {
        "title": "衢州徐員外使君遺以縞紵兼竹書箱因成一篇用答佳貺",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "爛柯山下舊仙郎，列宿來添婺女光。遠放歌聲分白紵，",
            "聞說天臺有遺愛，人將琪樹比甘棠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 27
    },
    {
        "title": "唐秀才贈端州紫石硯，以詩答之",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "端州石硯人間重，贈我因知正草玄。闕裏廟堂空舊物，",
            "此日傭工記名姓，因君數到墨池前。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 28
    },
    {
        "title": "覽董評事思歸之什因以詩贈",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "幾年油幕佐征東，卻泛滄浪狎釣童。欹枕醉眠成戲蝶，",
            "更說扁舟動鄉思，青菰已熟奈秋風。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 29
    },
    {
        "title": "謝寺雙檜",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "雙檜蒼然古貌奇，含煙吐霧鬱參差。晚依禪客當金殿，",
            "長明燈是前朝焰，曾照青青年少時。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 30
    },
    {
        "title": "寄楊八壽州",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "風獵紅旗入壽春，滿城歌舞向朱輪。八公山下清淮水，",
            "聖朝方用敢言者，次第應須舊諫臣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 31
    },
    {
        "title": "洛中寺北樓見賀監草書題詩",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "高樓賀監昔曾登，壁上筆蹤龍虎騰。中國書流尚皇象，",
            "唯恐塵埃轉磨滅，再三珍重囑山僧。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 32
    },
    {
        "title": "聞韓賓擢第歸覲以詩美之兼賀韓十五曹長時韓牧永州",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "零陵香草滿郊坰，丹穴雛飛入翠屏。孝若歸來成畫贊，",
            "為報儒林丈人道，如今從此鬢星星。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 33
    },
    {
        "title": "宣上人遠寄和禮部王侍郎放榜後詩，因而繼和",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "禮闈新榜動長安，九陌人人走馬看。一日聲名遍天下，",
            "借問至公誰印可，支郎天眼定中觀。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 34
    },
    {
        "title": "贈東嶽張煉師",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "東嶽真人張煉師，高情雅淡世間稀。堪為列女書青簡，",
            "雲衢不要吹簫伴，只擬乘鸞獨自飛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 35
    },
    {
        "title": "秘書崔少監見示墜馬長句，因而和之",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "麟台少監舊仙郎，洛水橋邊墜馬傷。塵汙腰間青襞綬，",
            "猶賴德全如醉者，不妨吟詠入篇章。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 36
    },
    {
        "title": "寄楊虢州與之舊姻",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "避地江湖知幾春，今來本郡擁朱輪。阮郎無複裏中舊，",
            "玉城山裏多靈藥，擺落功名且養神。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 37
    },
    {
        "title": "秋日題竇員外崇德里新居",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "長愛街西風景閑，到君居處暫開顏。清光門外一渠水，",
            "莫言堆案無餘地，認得詩人在此間。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 38
    },
    {
        "title": "蒙恩轉儀曹郎依前充集賢學士舉韓潮州自代因寄七言",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "翔鸞闕下謝恩初，通籍由來在石渠。暫入南宮判祥瑞，",
            "今日薦君嗟久滯，不惟文體似相如。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 39
    },
    {
        "title": "途次華州陪錢大夫登城北樓春望因睹李崔令狐三相國唱和之什翰林舊侶繼踵華城山水清高鸞鳳翔集皆忝宿眷遂題此詩",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "城樓四望出風塵，見盡關西渭北春。百二山河雄上國，",
            "莫怪老郎呈濫吹，宦途雖別舊情親。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 40
    },
    {
        "title": "始聞秋風",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "昔看黃菊與君別，今聽玄蟬我卻回。五夜颼飀枕前覺，",
            "天地肅清堪四望，為君扶病上高臺。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 41
    },
    {
        "title": "洛中初冬拜表有懷上京故人",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "鳳樓南面控三條，拜表郎官早渡橋。清洛曉光鋪碧簟，",
            "寄謝殷勤九天侶，搶榆水擊各逍遙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 42
    },
    {
        "title": "尉遲郎中見示自南遷牽複卻至洛城東舊居之作因以和之",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "曾遭飛語十年謫，新受恩光萬里還。朝服不妨遊洛浦，",
            "留作功成退身地，如今只是暫時閑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 43
    },
    {
        "title": "洛中酬福建陳判官見贈",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "潦倒聲名擁腫材，一生多故苦邅回。南宮舊籍遙相管，",
            "怪君近日文鋒利，新向延平看劍來。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 44
    },
    {
        "title": "和蘇十郎中謝病閒居時嚴常侍蕭給事同過訪歎初有二毛之作",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "清羸隱幾望雲空，左掖鴛鸞到室中。一卷素書消永日，",
            "莫怪人人驚早白，緣君尚是黑頭翁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 45
    },
    {
        "title": "酬淮南廖參謀秋夕見過之作",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "揚州從事夜相尋，無限新詩月下吟。初服已驚玄發長，",
            "不逐繁華訪閒散，知君擺落俗人心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 46
    },
    {
        "title": "題王郎中宣義裏新居",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "愛君新買街西宅，客到如游鄠杜間。雨後退朝貪種樹，",
            "見擬移居作鄰里，不論時節請開關。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 47
    },
    {
        "title": "酬朗州崔員外與任十四兄侍御同過鄙人舊居見懷之什時守吳郡",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "昔日居鄰招屈亭，楓林橘樹鷓鴣聲。一辭御苑青門去，",
            "何人萬里能相憶，同舍仙郎與外兄。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 48
    },
    {
        "title": "劉駙馬水亭避暑",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "千竿竹翠數蓮紅，水閣虛涼玉簟空。琥珀盞紅疑漏酒，",
            "盡日逍遙避煩暑，再三珍重主人翁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 49
    },
    {
        "title": "述舊賀遷寄陝虢孫常侍",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "南宮幸襲芝蘭後，左輔曾交印綬來。多病未離清洛苑，",
            "聞說隨車有零雨，此時偏動子荊才。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 50
    },
    {
        "title": "江陵嚴司空見示與成都武相公唱和，因命同作",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "南荊西蜀大行台，幕府旌門相對開。名重三司平水土，",
            "不是郢中清唱發，誰當丞相掞天才。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 51
    },
    {
        "title": "廟庭偃松詩",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "勢軋枝偏根已危，高情一見與扶持。忽從憔悴有生意，",
            "謝公莫道東山去，待取陰成滿鳳池。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 52
    },
    {
        "title": "贈致仕滕庶子先輩",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "朝服歸來晝錦榮，登科記上更無兄。壽觴每使曾孫獻，",
            "淩寒卻向山陰去，衣繡郎君雪裏行。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 53
    },
    {
        "title": "哭呂衡州，時予方謫居",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "一夜霜風凋玉芝，蒼生望絕士林悲。空懷濟世安人略，",
            "朔方徙歲行當滿，欲為君刊第二碑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 54
    },
    {
        "title": "夔州竇員外使君見示悼妓詩顧餘嘗識之因命同作",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "前年曾見兩鬟時，今日驚吟悼妓詩。鳳管學成知有籍，",
            "寂寞魚山青草裏，何人更立智瓊祠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 55
    },
    {
        "title": "竇夔州見寄寒食日憶故姬小紅吹笙，因和之",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "鸞聲窈眇管參差，清韻初調眾樂隨。幽院妝成花下弄，",
            "聞道今年寒食日，東山舊路獨行遲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 56
    },
    {
        "title": "哭龐京兆",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "俊骨英才氣褎然，策名飛步冠群賢。逢時已自致高位。",
            "得疾還因倚少年。天上別歸京兆府，人間空數茂陵阡。",
            "今朝繐帳哭君處，前日見鋪歌舞筵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 57
    },
    {
        "title": "送李庚先輩赴選",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "一家何啻十朱輪，諸父雙飛秉大鈞。曾脫素衣參幕客，",
            "今日山公舊賓主，知君不負帝城春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 58
    },
    {
        "title": "陽山廟觀賽神",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "漢家都尉舊征蠻，血食如今配此山，曲蓋幽深蒼檜下，",
            "日落風生廟門外，幾人連蹋竹歌還。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 59
    },
    {
        "title": "送僧元暠東游",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "寶書翻譯學初成，振錫如飛白足輕。彭澤因家凡幾世，",
            "從此多逢大居士，何人不願解珠瓔。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 60
    },
    {
        "title": "贈日本僧智藏",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "浮杯萬里過滄溟，遍禮名山適性靈。深夜降龍潭水黑，",
            "為問中華學道者，幾人雄猛得寧馨。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 61
    },
    {
        "title": "送元簡上人適越",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "孤雲出岫本無依，勝境名山即是歸。久向吳門遊好寺，",
            "更入天臺石橋去，垂珠璀璨拂三衣。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 62
    },
    {
        "title": "送宗密上人歸南山草堂寺，因謁河南尹白侍郎",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "宿習修來得慧根，多聞第一卻忘言。自從七祖傳心印，",
            "河南白尹大檀越，好把真經相對翻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 63
    },
    {
        "title": "西塞山懷古",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "西晉樓船下益州，金陵王氣黯然收。千尋鐵鎖沉江底，",
            "今逢四海為家日，故壘蕭蕭蘆荻秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 64
    },
    {
        "title": "廣宣上人寄在蜀與韋令公唱和詩卷因以令公手扎答詩示之",
        "author": "劉禹錫",
        "biography": "",
        "paragraphs": [
            "碧雲佳句久傳芳，曾向成都住草堂。振錫常過長者宅，",
            "若許相期同結社，吾家本自有柴桑。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷三百五十九",
        "no#": 65
    }
]