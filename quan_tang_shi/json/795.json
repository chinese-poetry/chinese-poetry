[
    {
        "title": "句",
        "author": "李日知",
        "biography": "滎陽人，歷相，中宗睿宗",
        "paragraphs": [
            "所願暫知居者樂，無使時稱主者勞。"
        ],
        "notes": [
            "（中宗幸安樂公主第，從官賦詩，日知卒章，獨存規誡，識者多之）"
        ],
        "volume": "卷七百九十五",
        "no#": 1
    },
    {
        "title": "句",
        "author": "趙仁奬",
        "biography": "河南人，景龍中為御史睿宗朝左授上蔡丞",
        "paragraphs": [
            "令乘驄馬去，丞脫繡衣來。"
        ],
        "notes": [
            "（《贈上蔡令潘好禮拜禦史》，見《朝野僉載》）"
        ],
        "volume": "卷七百九十五",
        "no#": 2
    },
    {
        "title": "句",
        "author": "郭廷謂",
        "biography": "",
        "paragraphs": [
            "魂逐東流水，墳依獨坐山。"
        ],
        "notes": [
            "（《哭陳子昂》）"
        ],
        "volume": "卷七百九十五",
        "no#": 3
    },
    {
        "title": "句",
        "author": "韋青",
        "biography": "官將軍",
        "paragraphs": [
            "三代掌綸誥，一身能唱歌。"
        ],
        "notes": [
            "（見《樂府雜錄》）"
        ],
        "volume": "卷七百九十五",
        "no#": 4
    },
    {
        "title": "句",
        "author": "尉遲匡",
        "biography": "幽并人，開元進士",
        "paragraphs": [
            "明月飛出海，黃河流上天。"
        ],
        "notes": [
            "（《暮行潼關》）"
        ],
        "volume": "卷七百九十五",
        "no#": 5
    },
    {
        "title": "句",
        "author": "尉遲匡",
        "biography": "",
        "paragraphs": [
            "芙蓉初出水，桃李忽無言。"
        ],
        "notes": [
            "（《觀內人樓上踏歌》）"
        ],
        "volume": "卷七百九十五",
        "no#": 6
    },
    {
        "title": "句",
        "author": "尉遲匡",
        "biography": "",
        "paragraphs": [
            "夜夜月為青塚鏡，年年雪作黑山花。"
        ],
        "notes": [
            "（《塞上曲》）"
        ],
        "volume": "卷七百九十五",
        "no#": 7
    },
    {
        "title": "句",
        "author": "何涓",
        "biography": "",
        "paragraphs": [
            "雁影數行秋半逢，漁歌一聲夜深發。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 8
    },
    {
        "title": "句",
        "author": "杜偉",
        "biography": "",
        "paragraphs": [
            "忽睹邢武辭，泠其金石備。"
        ],
        "notes": [
            "（《宣城總集》雲：唐開元甲子，武平一同河間邢巨同遊涇川琴溪，題絕句，古刻尚存。後一紀，杜偉自柱史謫掾宣城，陪連帥班景倩來觀，題句云云。餘逸）"
        ],
        "volume": "卷七百九十五",
        "no#": 9
    },
    {
        "title": "句",
        "author": "申堂構",
        "biography": "丹徒人，嘗為武進尉",
        "paragraphs": [
            "霜添柏樹冷，氣拂桂林寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 10
    },
    {
        "title": "句",
        "author": "周愿",
        "biography": "竟陵刺史",
        "paragraphs": [
            "八十年前棠樹陰，竟陵太守公先人。"
        ],
        "notes": [
            "（願與竟陵陸羽嘗佐嶺南連帥李複幕府。後願刺竟陵，則複已捐館，而羽亦前謝。複父齋物先亦為竟陵守，願因為七言詩陳事。見《方輿勝覽》）"
        ],
        "volume": "卷七百九十五",
        "no#": 11
    },
    {
        "title": "句",
        "author": "苐五琦",
        "biography": "字禹珪，長安人，肅宗朝為鹽鐵鑄錢使，拜相坐事長流終太子賔客",
        "paragraphs": [
            "陰天聞斷雁，夜浦送歸人。"
        ],
        "notes": [
            "（《送丘郎中》）"
        ],
        "volume": "卷七百九十五",
        "no#": 12
    },
    {
        "title": "句",
        "author": "顏允南",
        "biography": "歴官，司封郎中，國子司業",
        "paragraphs": [
            "誰言百人會，兄弟也沾陪。"
        ],
        "notes": [
            "（《侍宴》見《顏真卿集》）"
        ],
        "volume": "卷七百九十五",
        "no#": 13
    },
    {
        "title": "句",
        "author": "季廣琛",
        "biography": "𤓰州刺史，歴肅代朝終㪚騎常侍",
        "paragraphs": [
            "刻舟尋已化，彈鋏未酬恩。"
        ],
        "notes": [
            "（河西騎將宋青春有劍，是青龍精。刃所及，若叩銅鐵。青春死，為廣琛所得。或風雨後迸光出室，環燭方丈。哥舒翰求易以他寶，廣琛不與，因贈詩。見《酉陽雜俎》）"
        ],
        "volume": "卷七百九十五",
        "no#": 14
    },
    {
        "title": "句",
        "author": "陳蛻",
        "biography": "肅代間人",
        "paragraphs": [
            "夢裏換春秋。"
        ],
        "notes": [
            "（《詠華清宮》）"
        ],
        "volume": "卷七百九十五",
        "no#": 15
    },
    {
        "title": "句",
        "author": "衛準",
        "biography": "準一作單大，歴五年進士",
        "paragraphs": [
            "莫言閒話是閒話，往往事從閒話來。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 16
    },
    {
        "title": "句",
        "author": "衛準",
        "biography": "",
        "paragraphs": [
            "何必剃頭為弟子，無家便是出家人。"
        ],
        "notes": [
            "（並見張為《主客圖》）"
        ],
        "volume": "卷七百九十五",
        "no#": 17
    },
    {
        "title": "句",
        "author": "杜鴻漸",
        "biography": "字之㢲，濮陽人，累官朔方度支副使，以迎立肅宗，封衞國公代宗朝拜相",
        "paragraphs": [
            "常願追禪理，安能挹化源。"
        ],
        "notes": [
            "（鴻漸酷好浮屠道，晚年樂退靜，常悠然賦句，朝士多屬和之。見《本傳》）"
        ],
        "volume": "卷七百九十五",
        "no#": 18
    },
    {
        "title": "句",
        "author": "李摯",
        "biography": "貞元十二年宏詞科登第",
        "paragraphs": [
            "因緣三紀異，契分四般同。"
        ],
        "notes": [
            "（摯與李行敏同姓，同甲子、同年登第，俱二十五歲，又同門，故雲。見《紀事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 19
    },
    {
        "title": "句",
        "author": "韋綬",
        "biography": "德宗朝翰林學士",
        "paragraphs": [
            "在室愧屋漏。"
        ],
        "notes": [
            "（綬子溫疾革，誦綬此句曰：「今不負斯誡矣。」見《唐書》）"
        ],
        "volume": "卷七百九十五",
        "no#": 20
    },
    {
        "title": "句",
        "author": "馮戡",
        "biography": "東川人",
        "paragraphs": [
            "桃花浪裏成龍去，竹葉山頭退鷁飛。"
        ],
        "notes": [
            "（《贈柳棠及第》，見《紀事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 21
    },
    {
        "title": "句",
        "author": "韓泰",
        "biography": "字安平，昌黎人，戶部郎中，以附王叔文貶䖍州司馬後終郴州刺史",
        "paragraphs": [
            "庾嶺東邊吏隱州，溪山竹樹亦清幽。"
        ],
        "notes": [
            "（見《方輿勝覽》）"
        ],
        "volume": "卷七百九十五",
        "no#": 22
    },
    {
        "title": "句",
        "author": "盧並",
        "biography": "文宗朝資州守",
        "paragraphs": [
            "地靈無俗草，岩靜有仙禽。"
        ],
        "notes": [
            "（《等慈寺北岩》，見《方輿勝覽》）"
        ],
        "volume": "卷七百九十五",
        "no#": 23
    },
    {
        "title": "句",
        "author": "繆島雲",
        "biography": "島雲少從浮圖武宗朝，准勅，反初服",
        "paragraphs": [
            "白鳥遠行樹，玉虹孤飲潭。"
        ],
        "notes": [
            "（《廬山瀑布》）"
        ],
        "volume": "卷七百九十五",
        "no#": 24
    },
    {
        "title": "句",
        "author": "繆島雲",
        "biography": "",
        "paragraphs": [
            "四五片霞生絕壁，兩三行雁過疏鬆。"
        ],
        "notes": [
            "（以上見《紀事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 25
    },
    {
        "title": "句",
        "author": "繆島雲",
        "biography": "",
        "paragraphs": [
            "拋芥子降顛狒狒，折楊枝灑醉猩猩。"
        ],
        "notes": [
            "（見《摭言》）",
            "見《唐摭言》"
        ],
        "volume": "卷七百九十五",
        "no#": 26
    },
    {
        "title": "句",
        "author": "劉敬之",
        "biography": "雲安人",
        "paragraphs": [
            "山近衡陽雖少雁，水連巴蜀豈無魚。"
        ],
        "notes": [
            "（敬之甥雍陶，蜀川上第後，薄於親友，不寄書。敬之讓焉，陶得詩愧赧）"
        ],
        "volume": "卷七百九十五",
        "no#": 27
    },
    {
        "title": "句",
        "author": "李濤",
        "biography": "長沙人，溫飛卿任太學博士，主秋試。濤與衞丹、張郃等詩賦，皆牓於都",
        "paragraphs": [
            "水聲長在耳，山色不離門。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 28
    },
    {
        "title": "句",
        "author": "李濤",
        "biography": "",
        "paragraphs": [
            "掃地樹留影，拂床琴有聲。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 29
    },
    {
        "title": "句",
        "author": "李濤",
        "biography": "",
        "paragraphs": [
            "落日長安道，秋槐滿地花。"
        ],
        "notes": [
            "（以下並見《紀事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 30
    },
    {
        "title": "句",
        "author": "高元裕",
        "biography": "字景圭，渤海人，開成中翰林學士，終吏部尚書",
        "paragraphs": [
            "中丞為國拔賢才，寒俊欣逢藻鑒開。"
        ],
        "notes": [
            "（《贈知貢舉陳商》，見《池陽志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 31
    },
    {
        "title": "句",
        "author": "韋澳",
        "biography": "字子斐，大中初以考功郎，知制誥，充翰林學士承旨，終戶部侍郎",
        "paragraphs": [
            "莫將韋監同殷監，錯認容身是保身。"
        ],
        "notes": [
            "（懿宗朝，澳為吏部侍郎，不受請托，為執政所惡，出為邠寧節度。時宰復發其部事，罷鎮，以秘書監分司東都。嘗戲吟此句，京師權幸聞而尤怒之。見《唐書》）"
        ],
        "volume": "卷七百九十五",
        "no#": 32
    },
    {
        "title": "句",
        "author": "王龜",
        "biography": "字大年，官㑹稽太守",
        "paragraphs": [
            "珠箔卷繁星，金樽瀉月明。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 33
    },
    {
        "title": "句",
        "author": "李郁",
        "biography": "泉州人",
        "paragraphs": [
            "身死為修劉稹表，名高因讓陸洿書。"
        ],
        "notes": [
            "（《弔歐陽秬》秬，詹從子，開成中擢第。司勳郎中陸洿棄官隱吳中，後赴召複出，秬移書讓之，因不出。秬名益聞，竟赴澤潞劉稹幕辟。稹表指斥時政，朝廷疑秬所草，流崖州，賜死）"
        ],
        "volume": "卷七百九十五",
        "no#": 34
    },
    {
        "title": "句",
        "author": "詹雄",
        "biography": "字伯鎮，福州人，不第終",
        "paragraphs": [
            "塵飛遺恨盡，花落古宮平。"
        ],
        "notes": [
            "（《洛陽古城》）"
        ],
        "volume": "卷七百九十五",
        "no#": 35
    },
    {
        "title": "句",
        "author": "詹雄",
        "biography": "",
        "paragraphs": [
            "紅粉笙歌人代遠，月明陵樹水東流。"
        ],
        "notes": [
            "（《銅雀臺》）"
        ],
        "volume": "卷七百九十五",
        "no#": 36
    },
    {
        "title": "句",
        "author": "任濤",
        "biography": "豫章筠州人，咸通中十哲之一也，不第終",
        "paragraphs": [
            "露團沙鶴起，人臥釣船流。"
        ],
        "notes": [
            "（見《摭言》）"
        ],
        "volume": "卷七百九十五",
        "no#": 37
    },
    {
        "title": "句",
        "author": "劇燕",
        "biography": "蒲坂人，為詩雅正，亦十哲之一也。後客王重榮，被殺",
        "paragraphs": [
            "只向國門安四海，不離鄉井拜三公。"
        ],
        "notes": [
            "（《贈王重榮》。《紀事》：重榮鎮河中，燕投此詩，甚加禮敬。竟以淩轢諸從事，受正平之禍）"
        ],
        "volume": "卷七百九十五",
        "no#": 38
    },
    {
        "title": "句",
        "author": "王璘",
        "biography": "璘詞學富贍，崔詹事廉問表薦於朝，先試之使廨。璘請十書吏皆給筆札，璘口授十吏筆不停綴。日未亭午，已七千餘言。時路嵓方當鈞軸，遣一介召之。璘曰：請俟見帝。嵓大怒，亟命奏廢萬言科。璘杖䇿而歸",
        "paragraphs": [
            "芍藥花開菩薩面，棕閭葉散野人頭。"
        ],
        "notes": [
            "（璘與李群玉相遇嶽麓寺，群玉待之甚淺。曰：「請與公聯句。」破題而授之，璘略不佇思，繼之云云。群玉遂屈。見《紀事》）",
            "生平見《唐詩紀事》卷六六"
        ],
        "volume": "卷七百九十五",
        "no#": 39
    },
    {
        "title": "句",
        "author": "李蔚",
        "biography": "咸通中宰相，後出為節鎮",
        "paragraphs": [
            "虛心纖質雁銜餘，鳳吹龍吟定不如。"
        ],
        "notes": [
            "（薛陽陶善吹蘆管。蔚鎮淮海，陽陶為浙右小校，監押度支。運米至，蔚召，令出蘆管，於賞心亭奏之。蔚大嘉賞，贈詩此其終篇也）"
        ],
        "volume": "卷七百九十五",
        "no#": 40
    },
    {
        "title": "句",
        "author": "王枳",
        "biography": "常州刺史",
        "paragraphs": [
            "今朝拜貢盈襟淚，不進新芽是進心。"
        ],
        "notes": [
            "（常州舊貢陽羨茶。僖宗幸蜀，枳間關馳貢，故有此句。見《常州志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 41
    },
    {
        "title": "句",
        "author": "鄭賨",
        "biography": "字貢華，乾符進士",
        "paragraphs": [
            "萬蘊千牌次碧牙，縹箋金字間明霞。"
        ],
        "notes": [
            "（賨善楷書。天複中，挈家自華至陳，未嘗廢詞翰。其題經藏有此句，見《宣和書譜》）"
        ],
        "volume": "卷七百九十五",
        "no#": 42
    },
    {
        "title": "句",
        "author": "張塋",
        "biography": "速江人，大順初登第，官至禮部尚書",
        "paragraphs": [
            "一箭不中鵠，五湖歸釣魚。"
        ],
        "notes": [
            "（見《地誌》）"
        ],
        "volume": "卷七百九十五",
        "no#": 43
    },
    {
        "title": "句",
        "author": "吳靄",
        "biography": "字廷俊，連州人，光化三年進士，為朱忠幕僚",
        "paragraphs": [
            "煙隨紅焰斷，化作白雲飛。"
        ],
        "notes": [
            "（七歲時詠野燒，識者知其為青雲器）"
        ],
        "volume": "卷七百九十五",
        "no#": 44
    },
    {
        "title": "句",
        "author": "陳詠",
        "biography": "眉州青神人，善奕碁（圍棋），昭宗時登第",
        "paragraphs": [
            "隔岸水牛浮鼻渡，傍溪沙鳥點頭行。"
        ],
        "notes": [
            "（見《紀事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 45
    },
    {
        "title": "句",
        "author": "蔣密",
        "biography": "零陵儒士",
        "paragraphs": [
            "綺羅因片葉，桃李謾同時。"
        ],
        "notes": [
            "（詠桑 見《詩註總話》）"
        ],
        "volume": "卷七百九十五",
        "no#": 46
    },
    {
        "title": "句",
        "author": "符蒙",
        "biography": "字適之，後唐同光三年進士，官侍郎",
        "paragraphs": [
            "都緣心似水，故以缽為舟。"
        ],
        "notes": [
            "（《題壁畫杯渡道人》）"
        ],
        "volume": "卷七百九十五",
        "no#": 47
    },
    {
        "title": "句",
        "author": "李景遂",
        "biography": "字退身，元宗母弟，初封燕王，改封晉王",
        "paragraphs": [
            "路指丹陽分虎節，心存雙闕戀龍顏。"
        ],
        "notes": [
            "（《赴潤州鎮賜餞》，見《江表志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 48
    },
    {
        "title": "句",
        "author": "李弘茂",
        "biography": "",
        "paragraphs": [
            "甜於泉水茶須信，狂似楊花蝶未知。"
        ],
        "notes": [
            "（《詠雪》）"
        ],
        "volume": "卷七百九十五",
        "no#": 49
    },
    {
        "title": "句",
        "author": "李弘茂",
        "biography": "",
        "paragraphs": [
            "半窗月在猶煎藥，幾夜燈閑不照書。"
        ],
        "notes": [
            "（《病中》，並見《江表志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 50
    },
    {
        "title": "句",
        "author": "李平",
        "biography": "闗右人，本姓名曰楊訥，歸南唐，官尚書郎衛尉卿",
        "paragraphs": [
            "至人無夢夢不到，天道惡盈盈有餘。"
        ],
        "notes": [
            "（《閑書》）"
        ],
        "volume": "卷七百九十五",
        "no#": 51
    },
    {
        "title": "句",
        "author": "李平",
        "biography": "",
        "paragraphs": [
            "龍髯已斷嬪嬙老，豹尾不來岐路長。"
        ],
        "notes": [
            "（《讀武帝內傳》）"
        ],
        "volume": "卷七百九十五",
        "no#": 52
    },
    {
        "title": "句",
        "author": "胡元龜",
        "biography": "廬陵人，官臨川令",
        "paragraphs": [
            "翻身騰白浪，探爪擢明珠。"
        ],
        "notes": [
            "（永新令請題屏畫戲珠龍。令好受饋遺，元龜寫意譏之。令悟，追捕，遂亡入金陵）"
        ],
        "volume": "卷七百九十五",
        "no#": 53
    },
    {
        "title": "句",
        "author": "蔣鉤",
        "biography": "",
        "paragraphs": [
            "因借夢書過竹寺，學耕秋粟繞茆原。"
        ],
        "notes": [
            "（《寄柳宣》）"
        ],
        "volume": "卷七百九十五",
        "no#": 54
    },
    {
        "title": "句",
        "author": "蔣鉤",
        "biography": "",
        "paragraphs": [
            "芭蕉葉上無愁雨，自是多情聽斷腸。"
        ],
        "notes": [
            "（戎昱詩有「一夜不眠孤客耳，主人門外有芭蕉。」代為之答）"
        ],
        "volume": "卷七百九十五",
        "no#": 55
    },
    {
        "title": "句",
        "author": "史虛白",
        "biography": "",
        "paragraphs": [
            "風雨揭卻屋，全家醉不知。"
        ],
        "notes": [
            "（《詠漁父》　見《江南野録》）"
        ],
        "volume": "卷七百九十五",
        "no#": 56
    },
    {
        "title": "句",
        "author": "夏寶松",
        "biography": "",
        "paragraphs": [
            "孤猿叫落中巖月，野客吟殘半夜燈。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 57
    },
    {
        "title": "句",
        "author": "夏寶松",
        "biography": "",
        "paragraphs": [
            "雁飛南浦砧初斷，月滿西樓酒半醒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 58
    },
    {
        "title": "句",
        "author": "夏寶松",
        "biography": "",
        "paragraphs": [
            "曉來羸駟依前去，目斷遙山數點清。"
        ],
        "notes": [
            "（《宿江城》，並見《錦繡萬花谷》）"
        ],
        "volume": "卷七百九十五",
        "no#": 59
    },
    {
        "title": "句",
        "author": "趙慶",
        "biography": "水曹郎",
        "paragraphs": [
            "邁古文章金鸑鷟，出群行止玉麒麟。"
        ],
        "notes": [
            "（《贈邵拙》，見《馬令書》）"
        ],
        "volume": "卷七百九十五",
        "no#": 60
    },
    {
        "title": "句",
        "author": "潘天錫",
        "biography": "南唐時為省郎",
        "paragraphs": [
            "風便磬聲遠，日斜樓影長。"
        ],
        "notes": [
            "（《游古觀》，見《郡閣雅談》）"
        ],
        "volume": "卷七百九十五",
        "no#": 61
    },
    {
        "title": "句",
        "author": "朱勰",
        "biography": "南唐時，嘗為縣令",
        "paragraphs": [
            "好是晚來香雨裏，擔簦親送綺羅人。"
        ],
        "notes": [
            "（見《詩史》）"
        ],
        "volume": "卷七百九十五",
        "no#": 62
    },
    {
        "title": "句",
        "author": "齊鎬",
        "biography": "",
        "paragraphs": [
            "蓮中花更好，雲裏月常新。"
        ],
        "notes": [
            "（南唐宮嬪窅娘善舞，後主作金蓮，令窅娘纏足作新月狀，舞蓮中。鎬詩因窅娘作也。見《道山清話》）"
        ],
        "volume": "卷七百九十五",
        "no#": 63
    },
    {
        "title": "句",
        "author": "黃可",
        "biography": "字不可，南唐時人",
        "paragraphs": [
            "天下傳將舞馬賦，門前迎得跨驢賓。"
        ],
        "notes": [
            "（《獻高侍郎》）"
        ],
        "volume": "卷七百九十五",
        "no#": 64
    },
    {
        "title": "句",
        "author": "高元矩",
        "biography": "宣城人",
        "paragraphs": [
            "硯貯寒泉碧，庭堆敗葉紅。"
        ],
        "notes": [
            "（《贈宣城宰》）"
        ],
        "volume": "卷七百九十五",
        "no#": 65
    },
    {
        "title": "句",
        "author": "高元矩",
        "biography": "",
        "paragraphs": [
            "燕掠琴弦穿靜院，吏收詩草下閒庭。"
        ],
        "notes": [
            "（《贈徐學士》，見《雅言系述》）"
        ],
        "volume": "卷七百九十五",
        "no#": 66
    },
    {
        "title": "句",
        "author": "陳貺",
        "biography": "南唐處士",
        "paragraphs": [
            "年年聞爾者，未有不傷情。"
        ],
        "notes": [
            "（《蟬》）"
        ],
        "volume": "卷七百九十五",
        "no#": 67
    },
    {
        "title": "句",
        "author": "陳貺",
        "biography": "",
        "paragraphs": [
            "出得風塵者，合知岐路人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 68
    },
    {
        "title": "句",
        "author": "陳貺",
        "biography": "",
        "paragraphs": [
            "拂榻燈未來，開門月先入。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 69
    },
    {
        "title": "句",
        "author": "陳貺",
        "biography": "",
        "paragraphs": [
            "忽生雲是匣，高以月為臺。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 70
    },
    {
        "title": "句",
        "author": "陳貺",
        "biography": "",
        "paragraphs": [
            "入夜雖無傷物意，向明還有動人心。"
        ],
        "notes": [
            "（《畫虎》，並《見吟窗雜録》）"
        ],
        "volume": "卷七百九十五",
        "no#": 71
    },
    {
        "title": "句",
        "author": "趙休",
        "biography": "幸夤遜同時人",
        "paragraphs": [
            "金莖來白露，玉宇起清風。"
        ],
        "notes": [
            "（《侍宴》，見《吟窗雜録》）"
        ],
        "volume": "卷七百九十五",
        "no#": 72
    },
    {
        "title": "句",
        "author": "庸仁傑",
        "biography": "泉州人，初為僧。陳德誠勸之，反初服，官終汾陽令",
        "paragraphs": [
            "紅旆渡江霞蘸水，青蛇出匣雪侵衣。"
        ],
        "notes": [
            "（《贈池陽守陳德誠》）",
            "反初服：恢复做官时的服饰。出至《文選．曹植．七啟》：「願反初服，從子而歸。」"
        ],
        "volume": "卷七百九十五",
        "no#": 73
    },
    {
        "title": "句",
        "author": "庸仁傑",
        "biography": "",
        "paragraphs": [
            "雲散便凝千里望，日斜長占半城陰。"
        ],
        "notes": [
            "（《昇元閣》）"
        ],
        "volume": "卷七百九十五",
        "no#": 74
    },
    {
        "title": "句",
        "author": "庸仁傑",
        "biography": "",
        "paragraphs": [
            "只住此山甯有意，向來求佛本無心。"
        ],
        "notes": [
            "（《贈嘉禾峯僧》）",
            "甯:寜"
        ],
        "volume": "卷七百九十五",
        "no#": 75
    },
    {
        "title": "句",
        "author": "邵拙",
        "biography": "字拙之，宣城人",
        "paragraphs": [
            "萬國不得雨，孤雲猶在山。"
        ],
        "notes": [
            "（見《江南野録》）"
        ],
        "volume": "卷七百九十五",
        "no#": 76
    },
    {
        "title": "句",
        "author": "毛炳",
        "biography": "南唐時豐城人",
        "paragraphs": [
            "先生不在此，千載只空山。"
        ],
        "notes": [
            "（《題齋壁》，見《江南野錄》）"
        ],
        "volume": "卷七百九十五",
        "no#": 77
    },
    {
        "title": "句",
        "author": "陳德誠",
        "biography": "建州人，百勝軍節度使",
        "paragraphs": [
            "建水舊傳劉夜坐，螺川新有夏江城。"
        ],
        "notes": [
            "（劉洞有《夜坐》詩，夏寶松有《宿江城》詩皆見稱一時，號劉夜坐、夏江城雲）"
        ],
        "volume": "卷七百九十五",
        "no#": 78
    },
    {
        "title": "句",
        "author": "陳甫",
        "biography": "字惟岳，吉州人",
        "paragraphs": [
            "一雨洗殘暑，萬家生早涼。"
        ],
        "notes": [
            "（《漳江感懷》）"
        ],
        "volume": "卷七百九十五",
        "no#": 79
    },
    {
        "title": "句",
        "author": "陳甫",
        "biography": "",
        "paragraphs": [
            "暮鳥歸巢急，寒牛下隴遲。"
        ],
        "notes": [
            "（《村居》）"
        ],
        "volume": "卷七百九十五",
        "no#": 80
    },
    {
        "title": "句",
        "author": "陳甫",
        "biography": "",
        "paragraphs": [
            "算吟千百首，方得兩三聯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 81
    },
    {
        "title": "句",
        "author": "陳甫",
        "biography": "",
        "paragraphs": [
            "清時不作登龍客，綠鬢閑梳傍草堂。"
        ],
        "notes": [
            "（《贈黃岩》，並《雅言系述》）"
        ],
        "volume": "卷七百九十五",
        "no#": 82
    },
    {
        "title": "句",
        "author": "徐融",
        "biography": "南唐人",
        "paragraphs": [
            "淮船分蟻隊，江市聚蠅聲。"
        ],
        "notes": [
            "（《夜宿金山》，見《古今詩話》）"
        ],
        "volume": "卷七百九十五",
        "no#": 83
    },
    {
        "title": "句",
        "author": "高若拙",
        "biography": "荊南髙從誨幕客",
        "paragraphs": [
            "人間不見月，天外自分明。"
        ],
        "notes": [
            "（《中秋不見月》，見《大定錄》）",
            "曾在荊南文獻王高從誨擔任幕客"
            
        ],
        "volume": "卷七百九十五",
        "no#": 84
    },
    {
        "title": "句",
        "author": "皮光業",
        "biography": "字文通，日休之子，呉越署為推官，後拜相",
        "paragraphs": [
            "燒平樵路出，潮落海山高。"
        ],
        "notes": [
            "（見《葆光録》）"
        ],
        "volume": "卷七百九十五",
        "no#": 85
    },
    {
        "title": "句",
        "author": "皮光業",
        "biography": "",
        "paragraphs": [
            "行人折柳和輕絮，飛燕銜泥帶落花。"
        ],
        "notes": [
            "（見《西清詩話》）"
        ],
        "volume": "卷七百九十五",
        "no#": 86
    },
    {
        "title": "句",
        "author": "屠瓌智",
        "biography": "海鹽人，越州指揮贈節使上柱國",
        "paragraphs": [
            "輕身都是義，殉主始為忠。"
        ],
        "notes": [
            "（《詠志》）"
        ],
        "volume": "卷七百九十五",
        "no#": 87
    },
    {
        "title": "句",
        "author": "元德昭",
        "biography": "字名逺，杭州人，吳越相",
        "paragraphs": [
            "滿堂羅綺兼朱紫，四代兒孫奉老翁。"
        ],
        "notes": [
            "（德昭理家以孝愛聞，每時序置酒，環列幾席者凡四從，故其詩雲。見《備史》）"
        ],
        "volume": "卷七百九十五",
        "no#": 88
    },
    {
        "title": "句",
        "author": "林無隱",
        "biography": "閩人，寓明州吳越相鼎即其子也",
        "paragraphs": [
            "雪消二月江湖闊，花發千山道路香。"
        ],
        "notes": [
            "（見《備史》）",
            "吳越宰相林鼎之父"
        ],
        "volume": "卷七百九十五",
        "no#": 89
    },
    {
        "title": "句",
        "author": "楊義方",
        "biography": "眉山人，王建時舉進士",
        "paragraphs": [
            "海邊紅日半離水，天外暖風輕到花。"
        ],
        "notes": [
            "（《春日》）"
        ],
        "volume": "卷七百九十五",
        "no#": 90
    },
    {
        "title": "句",
        "author": "楊義方",
        "biography": "",
        "paragraphs": [
            "雨聲鞭自禁門出，一簇人從天上來。"
        ],
        "notes": [
            "（《贈王樞密處回》，見《異聞錄》）"
        ],
        "volume": "卷七百九十五",
        "no#": 91
    },
    {
        "title": "句",
        "author": "王廷珪",
        "biography": "",
        "paragraphs": [
            "十字水中分島嶼，數重花外見樓臺。"
        ],
        "notes": [
            "（浣花溪江皆創亭榭，孟昶遊之，曰：「曲江金殿鎖千門，未及此。」廷珪賦句，昶稱善。見《蜀檮杌》）"
        ],
        "volume": "卷七百九十五",
        "no#": 92
    },
    {
        "title": "句",
        "author": "歐陽彬",
        "biography": "字齊羙，衡山人。孟㫤時，為嘉州刺史，後至䕫州節度",
        "paragraphs": [
            "無錢將乞樊知客，名紙生毛不為通。"
        ],
        "notes": [
            "（彬初投謁馬殷，知客樊姓者索賄不得，不與通。後為《九州歌》幹之，又不問，因入蜀。見《南部新書》）"
        ],
        "volume": "卷七百九十五",
        "no#": 93
    },
    {
        "title": "句",
        "author": "歐陽彬",
        "biography": "",
        "paragraphs": [
            "桑柘殘陽裏，兒孫落葉中。"
        ],
        "notes": [
            "（彬有子稚齒，作《田父詩》云云。廖凝見之，知其不壽，尋果卒。見《郡閣雅談》）"
        ],
        "volume": "卷七百九十五",
        "no#": 94
    },
    {
        "title": "句",
        "author": "李堯夫",
        "biography": "蜀梓潼山人",
        "paragraphs": [
            "向外疑無地，其中別有天。"
        ],
        "notes": [
            "（《大內盆池》）"
        ],
        "volume": "卷七百九十五",
        "no#": 95
    },
    {
        "title": "句",
        "author": "李堯夫",
        "biography": "",
        "paragraphs": [
            "方外共推為道友，關中獨自占詩家。"
        ],
        "notes": [
            "（《贈滕郎中》）"
        ],
        "volume": "卷七百九十五",
        "no#": 96
    },
    {
        "title": "句",
        "author": "李堯夫",
        "biography": "",
        "paragraphs": [
            "炎暑鬱蒸無處避，涼風消息幾時來。"
        ],
        "notes": [
            "（見《古今詩話》）"
        ],
        "volume": "卷七百九十五",
        "no#": 97
    },
    {
        "title": "句",
        "author": "石文德",
        "biography": "連州人，仕長沙，歴水部員外郎、融州刺史",
        "paragraphs": [
            "月沈湘浦冷，花謝漢宮秋。"
        ],
        "notes": [
            "（《挽馬希范夫人》，見《五代史補》）"
        ],
        "volume": "卷七百九十五",
        "no#": 98
    },
    {
        "title": "句",
        "author": "戴偃",
        "biography": "金陵人，避地湘中，值希範大興土木。自稱𤣥黃子。獻《漁父詩》百篇以諷，希範怒。遷之湖上",
        "paragraphs": [
            "須把咽喉吞世界，蓋因奢侈致危亡。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 99
    },
    {
        "title": "句",
        "author": "戴偃",
        "biography": "",
        "paragraphs": [
            "若須拋卻便拋卻，莫待風高更亦深。"
        ],
        "notes": [
            "（並《漁父》篇。見《五代史補》）"
        ],
        "volume": "卷七百九十五",
        "no#": 100
    },
    {
        "title": "句",
        "author": "張迥",
        "biography": "湖南詩人",
        "paragraphs": [
            "夜長燈影滅，天繞鶴聲孤。"
        ],
        "notes": [
            "（見《吟窗雜録》）"
        ],
        "volume": "卷七百九十五",
        "no#": 101
    },
    {
        "title": "句",
        "author": "鍾元章",
        "biography": "南漢員外郎",
        "paragraphs": [
            "金筈離弦三尺電，星髇破的一聲雷。"
        ],
        "notes": [
            "（《吟窗雜録》）"
        ],
        "volume": "卷七百九十五",
        "no#": 102
    },
    {
        "title": "句",
        "author": "楊鳧",
        "biography": "字舄之，閩人",
        "paragraphs": [
            "背日流泉生凍早，逆風歸鳥入巢遲。"
        ],
        "notes": [
            "（《山中即事》，見《雅言系述》）"
        ],
        "volume": "卷七百九十五",
        "no#": 103
    },
    {
        "title": "句",
        "author": "王俊",
        "biography": "考功郎中",
        "paragraphs": [
            "勝日登樓望，山川一半春。"
        ],
        "notes": [
            "（《桂林逍遙樓》）"
        ],
        "volume": "卷七百九十五",
        "no#": 104
    },
    {
        "title": "句",
        "author": "薛沆",
        "biography": "廬州刺史",
        "paragraphs": [
            "也知別有風光主，花蕾枝枝似去年。"
        ],
        "notes": [
            "（《題藏舟浦花》，見《南部新書》）"
        ],
        "volume": "卷七百九十五",
        "no#": 105
    },
    {
        "title": "句",
        "author": "張顗",
        "biography": "官左司郎中",
        "paragraphs": [
            "金殿聖人看縱筆，玉堂詞客盡裁詩。"
        ],
        "notes": [
            "（《贈𧦬光》）"
        ],
        "volume": "卷七百九十五",
        "no#": 106
    },
    {
        "title": "句",
        "author": "張林",
        "biography": "擢進士第官至御史",
        "paragraphs": [
            "菱葉乍翻人采後，芰荷初沒舸行時。"
        ],
        "notes": [
            "（見《紀事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 107
    },
    {
        "title": "句",
        "author": "盧休",
        "biography": "",
        "paragraphs": [
            "春寒酒力遲，冉冉生微紅。"
        ],
        "notes": [
            "（《寒月》）"
        ],
        "volume": "卷七百九十五",
        "no#": 108
    },
    {
        "title": "句",
        "author": "盧休",
        "biography": "",
        "paragraphs": [
            "自然草木性，誰祝元化功。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 109
    },
    {
        "title": "句",
        "author": "盧休",
        "biography": "",
        "paragraphs": [
            "湓浦風生破膽愁。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 110
    },
    {
        "title": "句",
        "author": "盧休",
        "biography": "",
        "paragraphs": [
            "血染劍花明帳幕，三千車馬出漁陽。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 111
    },
    {
        "title": "句",
        "author": "盧休",
        "biography": "",
        "paragraphs": [
            "入門堪笑複堪憐，三徑苔荒一釣船。"
        ],
        "notes": [
            "（並見張為《主客圖》）"
        ],
        "volume": "卷七百九十五",
        "no#": 112
    },
    {
        "title": "句",
        "author": "陳嶠",
        "biography": "字景山，閩人，暮年登第，還鄉不仕",
        "paragraphs": [
            "小橋風月年年事，爭奈潘郎去□何。"
        ],
        "notes": [
            "見《南部新書》，第二句缺一字"
        ],
        "volume": "卷七百九十五",
        "no#": 113
    },
    {
        "title": "句",
        "author": "李範",
        "biography": "闗中人",
        "paragraphs": [
            "清猿啼遠木，白鳥下前灘。"
        ],
        "notes": [
            "《秋日江干逺望》"
        ],
        "volume": "卷七百九十五",
        "no#": 114
    },
    {
        "title": "句",
        "author": "李範",
        "biography": "",
        "paragraphs": [
            "鶴歸秋漢遠，人去草堂空。"
        ],
        "notes": [
            "《經王山人舊居》"
        ],
        "volume": "卷七百九十五",
        "no#": 115
    },
    {
        "title": "句",
        "author": "李範",
        "biography": "",
        "paragraphs": [
            "雖當南北路，不礙往來人。"
        ],
        "notes": [
            "《道傍樹》"
        ],
        "volume": "卷七百九十五",
        "no#": 116
    },
    {
        "title": "句",
        "author": "李範",
        "biography": "",
        "paragraphs": [
            "釣叟無機沙鳥睡，禪師入定白牛閑。"
        ],
        "notes": [
            "《江寺閑書》"
        ],
        "volume": "卷七百九十五",
        "no#": 117
    },
    {
        "title": "句",
        "author": "李範",
        "biography": "",
        "paragraphs": [
            "天涯故友無來信，窗外拒霜空落花。"
        ],
        "notes": [
            "（《暮秋懷故人》，並見《雅言雜載》）"
        ],
        "volume": "卷七百九十五",
        "no#": 118
    },
    {
        "title": "句",
        "author": "卞震",
        "biography": "蜀人",
        "paragraphs": [
            "雨壁長秋菌，風枝落病蟬。"
        ],
        "notes": [
            "《即事》"
        ],
        "volume": "卷七百九十五",
        "no#": 119
    },
    {
        "title": "句",
        "author": "卞震",
        "biography": "",
        "paragraphs": [
            "老笻搘瘦影，寒木憑吟身。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 120
    },
    {
        "title": "句",
        "author": "卞震",
        "biography": "",
        "paragraphs": [
            "詩債到春無處避，離愁因醉暫時無。"
        ],
        "notes": [
            "《春日偶題》"
        ],
        "volume": "卷七百九十五",
        "no#": 121
    },
    {
        "title": "句",
        "author": "卞震",
        "biography": "",
        "paragraphs": [
            "茶香解睡磨鐺煮，山色牽懷著屐登。"
        ],
        "notes": [
            "《即事》"
        ],
        "volume": "卷七百九十五",
        "no#": 122
    },
    {
        "title": "句",
        "author": "林楚才",
        "biography": "賀州富川人",
        "paragraphs": [
            "身閑不恨辭官早，詩好常甘得句遲。"
        ],
        "notes": [
            "（《贈致仕黃損》，見《雅言系述》"
        ],
        "volume": "卷七百九十五",
        "no#": 123
    },
    {
        "title": "句",
        "author": "孟不疑",
        "biography": "有詩名，溺於游覽，不復應舉",
        "paragraphs": [
            "白日故鄉遠，青山佳句中。"
        ],
        "notes": [
            "（見《酉陽雜俎》）"
        ],
        "volume": "卷七百九十五",
        "no#": 124
    },
    {
        "title": "句",
        "author": "龎季子",
        "biography": "",
        "paragraphs": [
            "冥雲生易滿，秋草長難高。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 125
    },
    {
        "title": "句",
        "author": "郭思",
        "biography": "",
        "paragraphs": [
            "落星一石幾千年，門外何人扣漢川。"
        ],
        "notes": [
            "（《白石鎮古城》）"
        ],
        "volume": "卷七百九十五",
        "no#": 126
    },
    {
        "title": "句",
        "author": "盧載",
        "biography": "",
        "paragraphs": [
            "五千里地望皆見，七十二峰中最高。"
        ],
        "notes": [
            "（《祝融峰》）"
        ],
        "volume": "卷七百九十五",
        "no#": 127
    },
    {
        "title": "句",
        "author": "鄭翱",
        "biography": "",
        "paragraphs": [
            "花燒落第眼，雨破到家程。"
        ],
        "notes": [
            "（下第東歸，見《海錄碎事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 128
    },
    {
        "title": "句",
        "author": "鄭說",
        "biography": "",
        "paragraphs": [
            "架上紫衣閑不著，案頭金字坐長看。"
        ],
        "notes": [
            "（《贈溫州大雲寺僧鴻楚》，見《高僧傳》）"
        ],
        "volume": "卷七百九十五",
        "no#": 129
    },
    {
        "title": "句",
        "author": "史瑜",
        "biography": "",
        "paragraphs": [
            "溪從沮水流嶓塚，嶺接青泥入劍天。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 130
    },
    {
        "title": "句",
        "author": "賀公",
        "biography": "石晉兵部",
        "paragraphs": [
            "但存方寸地，留與子孫耕。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 131
    },
    {
        "title": "句",
        "author": "米都知",
        "biography": "伶人，也善騷雅，梁補闕嘗贈以詩",
        "paragraphs": [
            "小旗村店酒，微雨野塘花。"
        ],
        "notes": [
            "騷雅：《離騷》與《詩經》中《大雅》﹑《小雅》的並稱",
            "補闕：唐代言官",
            "（見《南部新書》）"
        ],
        "volume": "卷七百九十五",
        "no#": 132
    },
    {
        "title": "句",
        "author": "陳秀才",
        "biography": "",
        "paragraphs": [
            "地偏雲自去，日暮山更深。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷七百九十五",
        "no#": 133
    },
    {
        "title": "句",
        "author": "崔李二生",
        "biography": "",
        "paragraphs": [
            "恰似傳花人飲散，空拋床下最繁枝。--崔",
            "豔魄香魂如有在，還應羞見墮樓人。--李"
        ],
        "notes": [
            "（二生並與武椽游。歩非煙死，同賦其事）",
            "崔李二生為唐代小說《非煙傳》中人物名，二人同與武公業游，武公業妾步非煙死，二人作詩吟其事"

        ],
        "volume": "卷七百九十五",
        "no#": 134
    },
    {
        "title": "句",
        "author": "范氏子",
        "biography": "《紀事》云，吳人攄子，七歲能詩。方干，見其《贈隠者》句，曰：「此子他日必成名。」又吟《夏日》句，干云：「必不享壽！」。果十歲卒。攄，即撰《雲溪友議》者",
        "paragraphs": [
            "掃葉隨風便，澆花趁日陰。"
        ],
        "notes": [
            "（《贈隠者》）",
            "范氏子：范攄之子",
            "攄：范攄",
            "見《堯山堂外紀：卷三十六唐》"
        ],
        "volume": "卷七百九十五",
        "no#": 135
    },
    {
        "title": "句",
        "author": "范氏子",
        "biography": "",
        "paragraphs": [
            "閑雲生不雨，病葉落非秋。"
        ],
        "notes": [
            "（《夏日》）"
        ],
        "volume": "卷七百九十五",
        "no#": 136
    },
    {
        "title": "句",
        "author": "臨川小吏",
        "biography": "李建勲鎮臨川，賞牡丹。有小吏捧硯，舉止有士人風。公曰：「學詩乎？」對曰：「麤親筆硯。」因令口占一篇。公竒之，勉令就學。因成詩名",
        "paragraphs": [
            "三月能辭千日醉，一生能得幾回看。"
        ],
        "notes": [
            "（見《詩話總龜》）",
            "見《詩話總龜前集》卷三"
        ],
        "volume": "卷七百九十五",
        "no#": 137
    },
    {
        "title": "句",
        "author": "韓熙載客",
        "biography": "",
        "paragraphs": [
            "最是五更留不住，向人枕畔著衣裳。"
        ],
        "notes": [
            "（熙載婢妾甚多，不為防閑，往往私出侍客，客賦詩云云。見《南唐近事》）"
        ],
        "volume": "卷七百九十五",
        "no#": 138
    },
    {
        "title": "句",
        "author": "段義宗",
        "biography": "外夷",
        "paragraphs": [
            "懸心秋夜月，萬里照鄉關。"
        ],
        "notes": [
            "（《思鄉》）"
        ],
        "volume": "卷七百九十五",
        "no#": 139
    },
    {
        "title": "句",
        "author": "段義宗",
        "biography": "",
        "paragraphs": [
            "玉排複道珊瑚殿，金錯危椆翡翠樓。"
        ],
        "notes": [
            "（《題三學院經樓》）"
        ],
        "volume": "卷七百九十五",
        "no#": 140
    },
    {
        "title": "句",
        "author": "段義宗",
        "biography": "",
        "paragraphs": [
            "此花不與眾花同，為感高僧護法功。"
        ],
        "notes": [
            "（《大慈寺芍藥》，並《吟窗雜録》）"
        ],
        "volume": "卷七百九十五",
        "no#": 141
    }
]