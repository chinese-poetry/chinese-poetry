[
    {
        "title": "聖泉宴",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "披襟乘石磴，列籍俯春泉。蘭氣熏山酌，松聲韻野弦。",
            "影飄垂葉外，香度落花前。興洽林塘晚，重岩起夕煙。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 1
    },
    {
        "title": "尋道觀",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "芝廛光分野，蓬闕盛規模。碧壇清桂閾，丹洞肅松樞。",
            "玉笈三山記，金箱五嶽圖。蒼虯不可得，空望白雲衢。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 2
    },
    {
        "title": "散關晨度",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "關山淩旦開，石路無塵埃。白馬高譚去，青牛真氣來。",
            "重門臨巨壑，連棟起崇隈。即今揚策度，非是棄繻回。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 3
    },
    {
        "title": "別薛華",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "送送多窮路，遑遑獨問津。悲涼千里道，淒斷百年身。",
            "心事同漂泊，生涯共苦辛。無論去與住，俱是夢中人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 4
    },
    {
        "title": "重別薛華",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "明月沉珠浦，風飄濯錦川。樓臺臨絕岸，洲渚亙長天。",
            "旅泊成千里，棲遑共百年。窮途唯有淚，還望獨潸然。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 5
    },
    {
        "title": "遊梵宇三覺寺",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "杏閣披青磴，雕台控紫岑。葉齊山路狹，花積野壇深。",
            "蘿幌棲禪影，松門聽梵音。遽忻陪妙躅，延賞滌煩襟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 6
    },
    {
        "title": "麻平晚行",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "百年懷土望，千里倦遊情。高低尋戍道，遠近聽泉聲。",
            "澗葉才分色，山花不辨名。羈心何處盡，風急暮猿清。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 7
    },
    {
        "title": "送盧主簿",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "窮途非所恨，虛室自相依。城闕居年滿，琴尊俗事稀。",
            "開襟方未已，分袂忽多違。東岩富松竹，歲暮幸同歸。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 8
    },
    {
        "title": "餞韋兵曹",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "征驂臨野次，別袂慘江垂。川霽浮煙斂，山明落照移。",
            "鷹風凋晚葉，蟬露泣秋枝。亭皋分遠望，延想間雲涯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 9
    },
    {
        "title": "白下驛餞唐少府",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "下驛窮交日，昌亭旅食年。相知何用早，懷抱即依然。",
            "浦樓低晚照，鄉路隔風煙。去去如何道，長安在日邊。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 10
    },
    {
        "title": "杜少府之任蜀州",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "城闕輔三秦，風煙望五津。與君離別意，同是宦遊人。",
            "海記憶體知己，天涯若比鄰。無為在岐路，兒女共沾巾。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 11
    },
    {
        "title": "仲春郊外",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "東園垂柳徑，西堰落花津。物色連三月，風光絕四鄰。",
            "鳥飛村覺曙，魚戲水知春。初晴山院裏，何處染囂塵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 12
    },
    {
        "title": "郊興",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "空園歌獨酌，春日賦閒居。澤蘭侵小徑，河柳覆長渠。",
            "雨去花光濕，風歸葉影疏。山人不惜醉，唯畏綠尊虛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 13
    },
    {
        "title": "郊園即事",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "煙霞春旦賞，松竹故年心。斷山疑畫障，縣溜瀉鳴琴。",
            "草遍南亭合，花開北院深。閒居饒酒賦，隨興欲抽簪。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 14
    },
    {
        "title": "觀佛跡寺",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "蓮座神容儼，松崖聖趾餘。年長金跡淺，地久石文疏。",
            "頹華臨曲磴，傾影赴前除。共嗟陵穀遠，俄視化城虛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 15
    },
    {
        "title": "山居晚眺贈王道士",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "金壇疏俗宇，玉洞侶仙群。花枝棲晚露，峰葉度晴雲。",
            "斜照移山影，回沙擁籀文。琴尊方待興，竹樹已迎曛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 16
    },
    {
        "title": "八仙徑",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "柰園欣八正，松岩訪九仙。援蘿窺霧術，攀林俯雲煙。",
            "代北鸞驂至，遼西鶴騎旋。終希脫塵網，連翼下芝田。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 17
    },
    {
        "title": "春日還郊",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "閒情兼嘿語，攜杖赴岩泉。草綠縈新帶，榆青綴古錢。",
            "魚床侵岸水，鳥路入山煙。還題平子賦，花樹滿春田。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 18
    },
    {
        "title": "對酒春園作",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "投簪下山閣，攜酒對河梁。狹水牽長鏡，高花送斷香。",
            "繁鶯歌似曲，疏蝶舞成行。自然催一醉，非但閱年光。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 19
    },
    {
        "title": "觀內懷仙",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "玉架殘書隱，金壇舊跡迷。牽花尋紫澗，步葉下清溪。",
            "瓊漿猶類乳，石髓尚如泥。自能成羽翼，何必仰雲梯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 20
    },
    {
        "title": "秋日別王長史",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "別路餘千里，深恩重百年。正悲西候日，更動北梁篇。",
            "野色籠寒霧，山光斂暮煙。終知難再奉，懷德自潸然。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 21
    },
    {
        "title": "上巳浮江宴韻得遙字",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "上巳年光促，中川興緒遙。綠齊山葉滿，紅泄片花銷。",
            "泉聲喧後澗，虹影照前橋。遽悲春望遠，江路積波潮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 22
    },
    {
        "title": "長柳",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "晨征犯煙磴，夕憩在雲關。晚風清近壑，新月照澄灣。",
            "郊童樵唱返，津叟釣歌還。客行無與晤，賴此釋愁顏。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 23
    },
    {
        "title": "銅雀妓二首",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "金鳳鄰銅雀，漳河望鄴城。君王無處所，台榭若平生。",
            "舞席紛何就，歌梁儼未傾。西陵松檟冷，誰見綺羅情。",
            "妾本深宮妓，層城閉九重。君王歡愛盡，歌舞為誰容。",
            "錦衾不復襞，羅衣誰再縫。高臺西北望，流涕向青松。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 24
    },
    {
        "title": "羈游餞別",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "客心懸隴路，遊子倦江幹。槿豐朝砌靜，筱密夜窗寒。",
            "琴聲銷別恨，風景駐離歡。寧覺山川遠，悠悠旅思難。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 25
    },
    {
        "title": "易陽早發",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "飭裝侵曉月，奔策候殘星。危閣尋丹障，回梁屬翠屏。",
            "雲間迷樹影，霧裏失峰形。複此涼飆至，空山飛夜螢。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 26
    },
    {
        "title": "焦岸早行和陸四",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "侵星違旅館，乘月戒征儔。複嶂迷晴色，虛岩辨暗流。",
            "猿吟山漏曉，螢散野風秋。故人渺何際，鄉關雲霧浮。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 27
    },
    {
        "title": "深灣夜宿",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "津塗臨巨壑，村宇架危岑。堰絕灘聲隱，風交樹影深。",
            "江童暮理楫，山女夜調砧。此時故鄉遠，甯知遊子心。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 28
    },
    {
        "title": "傷裴錄事喪子",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "蘭階霜候早，松露穸台深。魄散珠胎沒，芳銷玉樹沉。",
            "露文晞宿草，煙照慘平林。芝焚空歎息，流恨滿籝金。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 29
    },
    {
        "title": "泥溪",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "弭棹淩奔壑，低鞭躡峻岐。江濤出岸險，峰磴入雲危。",
            "溜急船文亂，岩斜騎影移。水煙籠翠渚，山照落丹崖。",
            "風生蘋浦葉，露泣竹潭枝。泛水雖雲美，勞歌誰複知。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 30
    },
    {
        "title": "三月曲水宴得煙字",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "彭澤官初去，河陽賦始傳。田園歸舊國，詩酒間長筵。",
            "列室窺丹洞，分樓瞰紫煙。縈回亙津渡，出沒控郊鄽。",
            "鳳琴調上客，龍轡儼群仙。松石偏宜古，藤蘿不記年。",
            "重簷交密樹，複磴擁危泉。抗石晞南嶺，乘沙眇北川。",
            "傅岩來築處，磻溪入釣前。日斜真趣遠，幽思夢涼蟬。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 31
    },
    {
        "title": "秋日仙游觀贈道士",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "石圖分帝宇，銀牒洞靈宮。回丹縈岫室，複翠上岩櫳。",
            "霧濃金灶靜，雲暗玉壇空。野花常捧露，山葉自吟風。",
            "林泉明月在，詩酒故人同。待余逢石髓，從爾命飛鴻。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 32
    },
    {
        "title": "晚留鳳州",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "寶雞辭舊役，仙鳳曆遺墟。去此近城闕，青山明月初。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 33
    },
    {
        "title": "羈春",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "客心千里倦，春事一朝歸。還傷北園裏，重見落花飛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 34
    },
    {
        "title": "林塘懷友",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "芳屏畫春草，仙杼織朝霞。何如山水路，對面即飛花。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 35
    },
    {
        "title": "山扉夜坐",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "抱琴開野室，攜酒對情人。林塘花月下，別似一家春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 36
    },
    {
        "title": "春莊",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "山中蘭葉徑，城外李桃園。豈知人事靜，不覺鳥聲喧。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 37
    },
    {
        "title": "春遊",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "客念紛無極，春淚倍成行。今朝花樹下，不覺戀年光。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 38
    },
    {
        "title": "春園",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "山泉兩處晚，花柳一園春。還持千日醉，共作百年人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 39
    },
    {
        "title": "林泉獨飲",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "丘壑經塗賞，花柳遇時春。相逢今不醉，物色自輕人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 40
    },
    {
        "title": "登城春望",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "物外山川近，晴初景靄新。芳郊花柳遍，何處不宜春。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 41
    },
    {
        "title": "他鄉敘興",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "綴葉歸煙晚，乘花落照春。邊城琴酒處，俱是越鄉人。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 42
    },
    {
        "title": "夜興",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "野煙含夕渚，山月照秋林。還將中散興，來偶步兵琴。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 43
    },
    {
        "title": "臨江二首",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "泛泛東流水，飛飛北上塵。歸驂將別棹，俱是倦遊人。",
            "去驂嘶別路，歸棹隱寒洲。江皋木葉下，應想故城秋。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 44
    },
    {
        "title": "江亭夜月送別二首",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "江送巴南水，山橫塞北雲。津亭秋月夜，誰見泣離群。",
            "亂煙籠碧砌，飛月向南端。寂寂離亭掩，江山此夜寒。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 45
    },
    {
        "title": "別人四首",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "久客逢餘閏，他鄉別故人。自然堪下淚，誰忍望征塵。",
            "江上風煙積，山幽雲霧多。送君南浦外，還望將如何。",
            "桂軺雖不駐，蘭筵幸未開。林塘風月賞，還待故人來。",
            "霜華淨天末，霧色籠江際。客子常畏人，何為久留滯。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 46
    },
    {
        "title": "贈李十四四首",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "野客思茅宇，山人愛竹林。琴尊唯待處，風月自相尋。",
            "小徑偏宜草，空庭不厭花。平生詩與酒，自得會仙家。",
            "亂竹開三徑，飛花滿四鄰。從來揚子宅，別有尚玄人。",
            "風筵調桂軫，月徑引藤杯。直當花院裏，書齋望曉開。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 47
    },
    {
        "title": "早春野望",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "江曠春潮白，山長曉岫青。他鄉臨睨極，花柳映邊亭。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 48
    },
    {
        "title": "山中",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "長江悲已滯，萬里念將歸。況屬高風晚，山山黃葉飛。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 49
    },
    {
        "title": "冬郊行望",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "桂密岩花白，梨疏林葉紅。江皋寒望盡，歸念斷征篷。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 50
    },
    {
        "title": "寒夜思友三首",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "久別侵懷抱，他鄉變容色。月下調鳴琴，相思此何極。",
            "雲間征思斷，月下歸愁切。鴻雁西南飛，如何故人別。",
            "朝朝翠山下，夜夜蒼江曲。複此遙相思，清尊湛芳綠。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 51
    },
    {
        "title": "始平晚息",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "觀闕長安近，江山蜀路賒。客行朝複夕，無處是鄉家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 52
    },
    {
        "title": "扶風晝屆離京浸遠",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "帝裏金莖去，扶風石柱來。山川殊未已，行路方悠哉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 53
    },
    {
        "title": "普安建陰題壁",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "江漢深無極，梁岷不可攀。山川雲霧裏，遊子幾時還。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 54
    },
    {
        "title": "九日",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "九日重陽節，開門有菊花。不知來送酒，若個是陶家。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 55
    },
    {
        "title": "秋江送別二首",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "早是他鄉值早秋，江亭明月帶江流。",
            "已覺逝川傷別念，複看津樹隱離舟。",
            "歸舟歸騎儼成行，江南江北互相望。",
            "誰謂波瀾才一水，已覺山川是兩鄉。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 56
    },
    {
        "title": "蜀中九日",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "九月九日望鄉台，他席他鄉送客杯。",
            "人情已厭南中苦，鴻雁那從北地來。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 57
    },
    {
        "title": "寒夜懷友雜體二首",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "北山煙霧始茫茫，南津霜月正蒼蒼。",
            "秋深客思紛無已，複值征鴻中夜起。",
            "複合重樓向浦開，秋風明月度江來。",
            "故人故情懷故宴，相望相思不相見。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 58
    },
    {
        "title": "落花落",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "落花落，落花紛漠漠。綠葉青跗映丹萼，",
            "落花飛，撩亂入中帷。落花春正滿，春人歸不歸。",
            "落花度，氛氳繞高樹。落花春已繁，春人春不顧。",
            "綺合青台靜且閑，羅袂紅巾複往還。盛年不再得，",
            "與君落花院，臺上起雙鬟。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 59
    },
    {
        "title": "九日懷封元寂",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "九日郊原望，平野遍霜威。蘭氣添新酌，花香染別衣。",
            "九秋良會少，千里故人稀。今日龍山外，當憶雁書歸。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 60
    },
    {
        "title": "出境遊山二首",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "源水終無路，山阿若有人。驅羊先動石，走兔欲投巾。",
            "洞晚秋泉冷，岩朝古樹新。峰斜連鳥翅，磴疊上魚鱗。",
            "化鶴千齡早，元龜六代春。浮雲今可駕，滄海自成塵。",
            "振翮淩霜吹，正月佇天潯。回鑣淩翠壑，飛軫控青岑。",
            "岩深靈灶沒，澗毀石渠沉。宮闕雲間近，江山物外臨。",
            "玉壇棲暮夜，珠洞結秋陰。蕭蕭離俗影，擾擾望鄉心。",
            "誰意山遊好，屢傷人事侵。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 61
    },
    {
        "title": "河陽橋代竇郎中佳人答楊中舍",
        "author": "王勃",
        "biography": "",
        "paragraphs": [
            "披風聽鳥長河路，臨津織女遙相妒。",
            "判知秋夕帶啼還，那及春朝攜手度。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 62
    },
    {
        "title": "晦日宴高氏林亭同用華字",
        "author": "王勔",
        "biography": "",
        "paragraphs": [
            "上序披林館，中京視物華。竹窗低露葉，梅徑起風花。",
            "景落春台霧，池侵舊渚沙。綺筵歌吹晚，暮雨泛香車。"
        ],
        "notes": [
            ""
        ],
        "volume": "卷五十六",
        "no#": 63
    }
]