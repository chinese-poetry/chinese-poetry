[
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "都來消帝力，全不用兵防。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 1
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "道遠擎空缽，深山踏落花。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 2
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "船中江上景，晚泊早行時。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 3
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "山寺鐘樓月，江城鼓角風。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 4
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "雨過花爭出，雲空月半生。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 5
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "鳥正啼隋柳，人須入楚山。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 6
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "高松飄雨雪，一室掩香燈。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 7
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "遂有冥心者，還尋此境來。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 8
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "又因風雨夜，重到古松門。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 9
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "道晦金雞伏，時來木馬鳴。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 10
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "誰來看山寺，自去掃松門。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 11
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "此生還自喜，餘事不相侵。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 12
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "白鬢無心鑷，青山得意多。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 13
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "一春能幾日，無雨亦多風。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 14
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "苦雨秋濤漲，狂風野燒翻。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 15
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "須知三尺劍，只為不平磨。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 16
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "要路爭先進，閑門肯暫過。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 17
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "已難消永夜，況複聽秋霖。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 18
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "大雪路亦宿，深山水也吞。"
        ],
        "notes": [
            "（贈僧，齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 19
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "捲簾黃葉落，鎖印子規啼。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 20
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "亡國空流水，孤祠掩落花。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 21
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "山魅隔窗舞，鵩鳥入簾飛。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 22
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "萬里八九月，一身西北風。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 23
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "磴危攀薜荔，石滑疏莓苔。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 24
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "日落無行客，天寒有去鴻。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 25
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "正思浮世事，又到古城邊。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 26
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "尋常風雨夜，應有鬼神看。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 27
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "寄宿山中寺，相辭海上僧。"
        ],
        "notes": [
            "（齊己《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 28
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "風吹榆莢葉，雨打木瓜花。"
        ],
        "notes": [
            "（炙轂子《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 29
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "是星皆拱北，無水不朝東。"
        ],
        "notes": [
            "（梅堯臣《續金針詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 30
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "西風起邊雁，一一向瀟湘。"
        ],
        "notes": [
            "（梅堯臣《續金針詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 31
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "那堪懷遠路，猶自上高樓。"
        ],
        "notes": [
            "（梅堯臣《續金針詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 32
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "白雲風散盡，紅葉水流來。"
        ],
        "notes": [
            "（徐寅《雅道機要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 33
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "妾如無異意，君棄也甘心。"
        ],
        "notes": [
            "（徐寅《雅道機要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 34
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "野過雲根闊，山高樹影長。"
        ],
        "notes": [
            "（徐寅《雅道機要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 35
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "古跡應重別，生緣不暫停。"
        ],
        "notes": [
            "（徐寅《雅道機要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 36
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "一個字未穩，數宵心不閑。"
        ],
        "notes": [
            "（徐寅《雅道機要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 37
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "別地葉頻落，去程山已寒。"
        ],
        "notes": [
            "（徐寅《雅道機要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 38
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "但取詩名得，何論下第頻。"
        ],
        "notes": [
            "（徐寅《雅道機要》）"
        ],
        "volume": "卷七百九十六",
        "no#": 39
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "早起赴前程，鄰雞尚未鳴。"
        ],
        "notes": [
            "（文彧《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 40
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "白地一回雨，兒孫拾得金。"
        ],
        "notes": [
            "（文彧《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 41
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "軒車在何處，雨雪滿前山。"
        ],
        "notes": [
            "（文彧《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 42
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "燈微山館雨，角咽海城秋。"
        ],
        "notes": [
            "（文彧《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 43
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "去帆看已遠，臨水立多時。"
        ],
        "notes": [
            "（《桂林詩評》）"
        ],
        "volume": "卷七百九十六",
        "no#": 44
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "家貧為客早，路遠得書稀。"
        ],
        "notes": [
            "（《桂林詩評》）"
        ],
        "volume": "卷七百九十六",
        "no#": 45
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "瀑布五千仞，草堂瀑布邊。"
        ],
        "notes": [
            "（《桂林詩評》）"
        ],
        "volume": "卷七百九十六",
        "no#": 46
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "我憶雲門寺，門前千萬峰。"
        ],
        "notes": [
            "（《桂林詩評》）"
        ],
        "volume": "卷七百九十六",
        "no#": 47
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "一瓶居無外，萬木老禪中。"
        ],
        "notes": [
            "（《桂林詩評》）"
        ],
        "volume": "卷七百九十六",
        "no#": 48
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "秋江洗一缽，寒日曬三衣。"
        ],
        "notes": [
            "（《桂林詩評》）"
        ],
        "volume": "卷七百九十六",
        "no#": 49
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "空山行客少，獨樹晚蟬多。"
        ],
        "notes": [
            "（《桂林詩評》）"
        ],
        "volume": "卷七百九十六",
        "no#": 50
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "一片月生海，幾家人上樓。"
        ],
        "notes": [
            "（待月，王正字《詩中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 51
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "窗寒風漸緊，燈落夜將深。"
        ],
        "notes": [
            "（秋日言懷，王正字詩中旨格）"
        ],
        "volume": "卷七百九十六",
        "no#": 52
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "雲夢幾行去，瀟湘一夜空。"
        ],
        "notes": [
            "（送雁，王正字《詩中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 53
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "無風來竹院，有月在莎庭。"
        ],
        "notes": [
            "（蟋蟀，王正字《詩中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 54
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "故園從小別，夜雨近秋聞。"
        ],
        "notes": [
            "（王正字《詩中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 55
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "書劍別南州，炎荒萬里遊。"
        ],
        "notes": [
            "（王正字《詩中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 56
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "山盤樵徑上，人到雪房遲。"
        ],
        "notes": [
            "（王正字《詩中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 57
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "路經秋雨後，人入亂峰來。"
        ],
        "notes": [
            "（王正字《詩中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 58
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "獨樹知秋早，孤舟覺夜寒。"
        ],
        "notes": [
            "（王正字《詩中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 59
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "馬饑餐落葉，鶴宿曬殘陽。"
        ],
        "notes": [
            "（王正字《詩中旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 60
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "路遠春行盡，家貧愁到時。"
        ],
        "notes": [
            "（《吟窗雜錄》）"
        ],
        "volume": "卷七百九十六",
        "no#": 61
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "臨水通宵坐，知君此興同。"
        ],
        "notes": [
            "（《吟窗雜錄》）"
        ],
        "volume": "卷七百九十六",
        "no#": 62
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "江聲兼小雨，暝色入啼猿。"
        ],
        "notes": [
            "（《樹萱錄》）"
        ],
        "volume": "卷七百九十六",
        "no#": 63
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "藕隱玲瓏玉，花藏縹緲容。"
        ],
        "notes": [
            "（《樹萱錄》）"
        ],
        "volume": "卷七百九十六",
        "no#": 64
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "紅樹醉秋色，碧溪彈夜弦。"
        ],
        "notes": [
            "（《樹萱錄》）"
        ],
        "volume": "卷七百九十六",
        "no#": 65
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "網斷蛛猶織，梁空燕不歸。"
        ],
        "notes": [
            "（《樹萱錄》）"
        ],
        "volume": "卷七百九十六",
        "no#": 66
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "倒身無著處，呵手不成溫。"
        ],
        "notes": [
            "（《苦寒》見《事文類聚》）"
        ],
        "volume": "卷七百九十六",
        "no#": 67
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "幾多詩弟子，無限酒知聞。"
        ],
        "notes": [
            "（《悼方幹》見《海錄碎事》）"
        ],
        "volume": "卷七百九十六",
        "no#": 68
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "床上小熏籠，韶州新退紅。"
        ],
        "notes": [
            "（《樂府》見陸務觀集注）"
        ],
        "volume": "卷七百九十六",
        "no#": 69
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "看時人步澀，展處蝶爭來。"
        ],
        "notes": [
            "（《題於兢畫牡丹》，"
        ],
        "volume": "卷七百九十六",
        "no#": 70
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "兢字德源，長安人，梁宰相也。"
        ],
        "notes": [
            "見《見聞志》）"
        ],
        "volume": "卷七百九十六",
        "no#": 71
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "酒味雜蓮氣，香冷勝於冰。"
        ],
        "notes": [
            "輪囷如象鼻，瀟灑絕青蠅。（《碧筩杯》，見楊慎《藝林伐山》）"
        ],
        "volume": "卷七百九十六",
        "no#": 72
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "太宗皇帝真長策，賺得英雄盡白頭。"
        ],
        "notes": [
            "（《國史補》曰：進士科得之艱難，其有老死於文場者，亦無所恨，故詩雲。《畫墁錄》以為趙嘏作）"
        ],
        "volume": "卷七百九十六",
        "no#": 73
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "曾題名處添前字，送出城人乞舊衣。"
        ],
        "notes": [
            "（神龍已來，杏園宴後，皆於慈恩塔下題名，及第後知聞。或遇未及第時題名處，則為添前字，故詩雲。見《摭言》）"
        ],
        "volume": "卷七百九十六",
        "no#": 74
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "華陽觀裏鐘聲集，建福門前鼓動期。"
        ],
        "notes": [
            "（新進士放榜後翌日，排光範門候過宰相。雖雲排建福門，實集於西方館，故詩云云。見《南部新書》）"
        ],
        "volume": "卷七百九十六",
        "no#": 75
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "隴右諸侯供語鳥，日南太守送名花。"
        ],
        "notes": [
            "（李德裕營平泉，遠方之人，多以異物奉之，時有題詩云云。見《劇談錄》）"
        ],
        "volume": "卷七百九十六",
        "no#": 76
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "八百孤寒齊下淚，一時回首望崖州。"
        ],
        "notes": [
            "（德裕為相，頗為寒進開路。及謫官南去，人詠此。見《摭言》）"
        ],
        "volume": "卷七百九十六",
        "no#": 77
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "讓劫已令多士伏，沽名還得世人聞。"
        ],
        "notes": [
            "（陳詠有詩名，善弈棋。昭宗幸陝之歲，策名歸蜀，韋莊以詩賀之。鄉中有嫉善者，屬和韋詩云云，比之滌器當壚也。見《紀事》）"
        ],
        "volume": "卷七百九十六",
        "no#": 78
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "人為子推初禁火，花愁青女再飛霜。"
        ],
        "notes": [
            "（《寒食》，見《事文類聚》）"
        ],
        "volume": "卷七百九十六",
        "no#": 79
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "楝花開後風光好，梅子黃時雨意濃。"
        ],
        "notes": [
            " （江南自初春至初夏，有二十四番花信風，最後為楝花風，故唐人有此句。見《東皋雜錄》）"
        ],
        "volume": "卷七百九十六",
        "no#": 80
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "蘭湯備浴傳荊俗，水馬浮江吊屈魂。"
        ],
        "notes": [
            "（唐人《午日》詩。南方競渡，治其舟使輕利，名飛鳧，名水車，又名水馬。見《荊楚歲時記》）"
        ],
        "volume": "卷七百九十六",
        "no#": 81
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "城頭椎鼓傳花枝，席上摶拳握松子。"
        ],
        "notes": [
            "（見《東皋雜錄》）"
        ],
        "volume": "卷七百九十六",
        "no#": 82
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "鳥向不香花裏宿，人從無影月中歸。"
        ],
        "notes": [
            "（《雪》）"
        ],
        "volume": "卷七百九十六",
        "no#": 83
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "輕著凍痕銷水面，密隨和氣入花根。"
        ],
        "notes": [
            "（《春雪》）"
        ],
        "volume": "卷七百九十六",
        "no#": 84
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "萬木盡如花落候，四簷鳴□雨來時。"
        ],
        "notes": [
            "（《晴雪》，王安石嘗舉此三聯，雲唐人詩）"
        ],
        "volume": "卷七百九十六",
        "no#": 85
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "濃綠萬枝紅一點，動人春色不須多。"
        ],
        "notes": [
            "（王安石在翰苑，見榴花止開一朵，有詩。陳正敏謂此乃唐人詩，非安石所作。見《泊宅編》）"
        ],
        "volume": "卷七百九十六",
        "no#": 86
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "晚菘細切肥牛肚，新筍初嘗嫩馬蹄。"
        ],
        "notes": [
            "（見《錦繡萬花穀》）"
        ],
        "volume": "卷七百九十六",
        "no#": 87
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "蟬離楚樹鳴猶少，葉到嵩山落更多。"
        ],
        "notes": [
            "（《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 88
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "風和日暖方開眼，雨潤煙濃不舉頭。"
        ],
        "notes": [
            "（《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 89
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "窗前閑詠鴛鴦句，壁上時觀獬廌圖。"
        ],
        "notes": [
            "（《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 90
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "可能有事關心後，得似無人識面時。"
        ],
        "notes": [
            "（《風騷旨格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 91
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "一點孤燈人夢覺，萬重寒葉雨聲多。"
        ],
        "notes": [
            "（炙轂子《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 92
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "此心只待相逢說，時複登樓看遠山。"
        ],
        "notes": [
            "（炙轂子《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 93
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "九江有浪船難濟，三峽無猿客有愁。"
        ],
        "notes": [
            "（炙轂子《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 94
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "四海魚龍精魄冷，五山鸞鳳骨毛寒。"
        ],
        "notes": [
            "（炙轂子《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 95
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "誰家綠酒歡留客，何處紅樓睡失明。"
        ],
        "notes": [
            "（炙轂子《詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 96
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "雨露施恩無厚薄，蓬蒿隨分有枯榮。"
        ],
        "notes": [
            "（梅堯臣《續金針詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 97
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "去年花下留連飲，暖日夭桃鶯亂啼。"
        ],
        "notes": [
            "（梅堯臣《續金針詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 98
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "今日江邊容易別，淡煙衰草馬頻嘶。"
        ],
        "notes": [
            "（梅堯臣《續金針詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 99
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "蛇蠍性靈生便毒，蕙蘭根異死猶香。"
        ],
        "notes": [
            "（梅堯臣《續金針詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 100
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "三間茅屋無人到，十裏松門獨自遊。"
        ],
        "notes": [
            "（梅堯臣《續金針詩格》）"
        ],
        "volume": "卷七百九十六",
        "no#": 101
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "翻憶舊山青碧裏，繞庵閑伴野僧禪。"
        ],
        "notes": [
            "（《雲》）"
        ],
        "volume": "卷七百九十六",
        "no#": 102
    },
    {
        "title": "句",
        "author": "無名氏",
        "biography": "",
        "paragraphs": [
            "不甘五等諸侯薦，直肯九重天子知。"
        ],
        "notes": [
            "（劉素者，字仲華。好學，不事科舉，頗通遷、固、壽、曄之書，嘗有人貽詩云云，然卒不及仕。見《江南野志》）"
        ],
        "volume": "卷七百九十六",
        "no#": 103
    }
]